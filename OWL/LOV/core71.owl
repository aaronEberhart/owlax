    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://w3id.org/amlo/core#RiskEstimator -->

    <rdfs:Datatype rdf:about="http://w3id.org/amlo/core#RiskEstimator">
        <rdfs:comment xml:lang="it">un valore probabilistico che esclude certezze</rdfs:comment>
        <owl:equivalentClass>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">0</xsd:minInclusive>
                    </rdf:Description>
                    <rdf:Description>
                        <xsd:maxInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">1</xsd:maxInclusive>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </owl:equivalentClass>
    </rdfs:Datatype>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://w3id.org/amlo/core#assessedOn -->

    <owl:ObjectProperty rdf:about="http://w3id.org/amlo/core#assessedOn">
        <owl:inverseOf rdf:resource="http://w3id.org/amlo/core#hasEvaluation"/>
        <rdfs:comment xml:lang="it">si riferisce all&apos;oggetto di una valutazione</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/amlo/core#detects -->

    <owl:ObjectProperty rdf:about="http://w3id.org/amlo/core#detects">
        <owl:inverseOf rdf:resource="http://w3id.org/amlo/core#isDetectedBy"/>
        <rdfs:domain rdf:resource="http://w3id.org/amlo/core/DetectionEvent"/>
        <rdfs:range rdf:resource="http://w3id.org/amlo/core#UnexpectedActivity"/>
        <rdfs:comment xml:lang="it">rileva una operatività inattesa</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/amlo/core#hasEvaluation -->

    <owl:ObjectProperty rdf:about="http://w3id.org/amlo/core#hasEvaluation">
        <rdfs:comment xml:lang="it">collega un soggetto ad una valutazione</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/amlo/core#hasGender -->

    <owl:ObjectProperty rdf:about="http://w3id.org/amlo/core#hasGender">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/AgentsAndPeople/People/Person"/>
        <rdfs:range rdf:resource="http://w3id.org/amlo/core#Gender"/>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/amlo/core#hasSizeEstimator -->

    <owl:ObjectProperty rdf:about="http://w3id.org/amlo/core#hasSizeEstimator">
        <rdfs:comment xml:lang="it">collega una stima quantitativa ad un rischio</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/amlo/core#hasSuspectRatingScore -->

    <owl:ObjectProperty rdf:about="http://w3id.org/amlo/core#hasSuspectRatingScore">
        <rdfs:subPropertyOf rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Arrangements/Ratings/hasRatingScore"/>
        <rdfs:range rdf:resource="http://w3id.org/amlo/core#SuspectRatingScore"/>
        <rdfs:comment xml:lang="it">collega un valore qualitativo espresso su un rischio</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/amlo/core#isDetectedBy -->

    <owl:ObjectProperty rdf:about="http://w3id.org/amlo/core#isDetectedBy">
        <rdfs:domain rdf:resource="http://w3id.org/amlo/core#UnexpectedActivity"/>
        <rdfs:comment xml:lang="it">è rilevata da une evento di ricerca operatività sospetta</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/amlo/core#isRecordedIn -->

    <owl:ObjectProperty rdf:about="http://w3id.org/amlo/core#isRecordedIn">
        <rdfs:domain rdf:resource="http://w3id.org/amlo/core#Transfer"/>
        <rdfs:comment xml:lang="it">identifica un libro utilizzato per registrare un trasferimento</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/amlo/core#onRiskFactor -->

    <owl:ObjectProperty rdf:about="http://w3id.org/amlo/core#onRiskFactor">
        <rdfs:range rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <rdfs:comment xml:lang="it">collega qualcosa ald un concetto di rischio astratto</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/amlo/core#operatesIn -->

    <owl:ObjectProperty rdf:about="http://w3id.org/amlo/core#operatesIn">
        <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/AgentsAndPeople/Agents/AutonomousAgent"/>
        <rdfs:range rdf:resource="https://www.omg.org/spec/LCC/Countries/CountryRepresentation/GeopoliticalEntity"/>
        <rdfs:comment xml:lang="en">states that the annotated subject operates in the related geopolitical entity</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/amlo/core#transfers -->

    <owl:ObjectProperty rdf:about="http://w3id.org/amlo/core#transfers">
        <rdfs:domain rdf:resource="http://w3id.org/amlo/core#Transfer"/>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment xml:lang="it">trasferisce qualcosa</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/amlo/core#wasBorn -->

    <owl:ObjectProperty rdf:about="http://w3id.org/amlo/core#wasBorn">
        <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/AgentsAndPeople/Agents/AutonomousAgent"/>
        <rdfs:range rdf:resource="https://www.omg.org/spec/LCC/Countries/CountryRepresentation/GeopoliticalEntity"/>
        <rdfs:comment xml:lang="en">states that a person or an organization was bornor registered in the related geopolitical entity (municipality, country etc.)</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://spec.edmcouncil.org/fibo/ontology/FND/AgentsAndPeople/Agents/isIdentifiedBy -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/ontology/FND/AgentsAndPeople/Agents/isIdentifiedBy"/>
    


    <!-- https://spec.edmcouncil.org/fibo/ontology/FND/Arrangements/Ratings/hasRatingScore -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/ontology/FND/Arrangements/Ratings/hasRatingScore"/>
    


    <!-- https://spec.edmcouncil.org/fibo/ontology/FND/Arrangements/Reporting/isReportedTo -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/ontology/FND/Arrangements/Reporting/isReportedTo"/>
    


    <!-- https://spec.edmcouncil.org/fibo/ontology/FND/Arrangements/Reporting/reportsOn -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/ontology/FND/Arrangements/Reporting/reportsOn"/>
    


    <!-- https://spec.edmcouncil.org/fibo/ontology/FND/Parties/Parties/hasPartyInRole -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/ontology/FND/Parties/Parties/hasPartyInRole"/>
    


    <!-- https://spec.edmcouncil.org/fibo/ontology/FND/Quantities/QuantitiesAndUnits/hasQuantityValue -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/ontology/FND/Quantities/QuantitiesAndUnits/hasQuantityValue"/>
    


    <!-- https://spec.edmcouncil.org/fibo/ontology/FND/Relations/Relations/comprises -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/ontology/FND/Relations/Relations/comprises"/>
    


    <!-- https://spec.edmcouncil.org/fibo/ontology/FND/Relations/Relations/hasIdentity -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/ontology/FND/Relations/Relations/hasIdentity"/>
    


    <!-- https://spec.edmcouncil.org/fibo/ontology/FND/Relations/Relations/involves -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/ontology/FND/Relations/Relations/involves"/>
    


    <!-- https://spec.edmcouncil.org/fibo/ontology/FND/Relations/Relations/isProvidedBy -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/fibo/ontology/FND/Relations/Relations/isProvidedBy"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://w3id.org/amlo/core#hasRiskEstimator -->

    <owl:DatatypeProperty rdf:about="http://w3id.org/amlo/core#hasRiskEstimator">
        <rdfs:range rdf:resource="http://w3id.org/amlo/core#RiskEstimator"/>
        <rdfs:comment xml:lang="it">un valore copreso tra 0 e 1 che rappresenta lo stimatore di un rischio</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/amlo/core#hasTransferDescription -->

    <owl:DatatypeProperty rdf:about="http://w3id.org/amlo/core#hasTransferDescription">
        <rdfs:domain rdf:resource="http://w3id.org/amlo/core#Transfer"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="it">la descrizione testuale associata ad un bonifico</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/amlo/core/wasEstimatedOnDate -->

    <owl:DatatypeProperty rdf:about="http://w3id.org/amlo/core/wasEstimatedOnDate">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://w3id.org/amlo/core/ConventionalMoneyValue"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment xml:lang="it">data in cui un vallore è stato stimato</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/BusinessRegistries/hasCity -->

    <owl:DatatypeProperty rdf:about="https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/BusinessRegistries/hasCity"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://w3id.org/amlo/core#Accountable -->

    <owl:Class rdf:about="http://w3id.org/amlo/core#Accountable">
        <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Parties/Parties/PartyInRole"/>
        <rdfs:comment xml:lang="it">identifica un agente come   responsabile di una operatività inattesa</rdfs:comment>
    </owl:Class>
    


    <!-- http://w3id.org/amlo/core#Agent -->

    <owl:Class rdf:about="http://w3id.org/amlo/core#Agent">
        <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/AgentsAndPeople/Agents/AutonomousAgent"/>
        <rdfs:comment xml:lang="en">An agent is an autonomous individual that can be involved in AML ativities</rdfs:comment>
    </owl:Class>
    


    <!-- http://w3id.org/amlo/core#Analyst -->

    <owl:Class rdf:about="http://w3id.org/amlo/core#Analyst">
        <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Arrangements/Ratings/RatingIssuer"/>
        <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Arrangements/Ratings/RatingParty"/>
        <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Parties/Parties/PartyInRole"/>
        <rdfs:comment xml:lang="it">identifica un agente preposto alla valutazione di una operatività inattesa</rdfs:comment>
    </owl:Class>
    


    <!-- http://w3id.org/amlo/core#Book -->

    <owl:Class rdf:about="http://w3id.org/amlo/core#Book">
        <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Parties/Roles/ThingInRole"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Relations/Relations/hasIdentity"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Relations/Relations/comprises"/>
                        <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Arrangements/Documents/Record"/>
                    </owl:Restriction>
                </owl:onClass>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="it">identifica un qualsiasi registro usato per annotare record</rdfs:comment>
    </owl:Class>
    


    <!-- http://w3id.org/amlo/core#Cash -->

    <owl:Class rdf:about="http://w3id.org/amlo/core#Cash">
        <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Accounting/CurrencyAmount/AmountOfMoney"/>
        <rdfs:comment xml:lang="it">la somma di  ammotare di denaro contante. Per rifere  una somma  di denaro astratta (ad esempio quello trasferito con un bonifico) bisogna  la classe MonetaryAmount.</rdfs:comment>
    </owl:Class>
    


    <!-- http://w3id.org/amlo/core#Gender -->

    <owl:Class rdf:about="http://w3id.org/amlo/core#Gender">
        <rdfs:comment>A person Gender</rdfs:comment>
        <rdfs:label></rdfs:label>
    </owl:Class>
    


    <!-- http://w3id.org/amlo/core#IntangibleAsset -->

    <owl:Class rdf:about="http://w3id.org/amlo/core#IntangibleAsset">
        <rdfs:comment xml:lang="it">qualsiasi bene intangibile che non sia denaro o immobili</rdfs:comment>
    </owl:Class>
    


    <!-- http://w3id.org/amlo/core#OriginatorBook -->

    <owl:Class rdf:about="http://w3id.org/amlo/core#OriginatorBook">
        <rdfs:subClassOf rdf:resource="http://w3id.org/amlo/core#Book"/>
        <rdfs:comment xml:lang="it">identifica un registro  (es. libro mastro) usato  usato per annotare un trasferimento  da parte dell&apos;ordinate</rdfs:comment>
    </owl:Class>
    


    <!-- http://w3id.org/amlo/core#OriginatorParty -->

    <owl:Class rdf:about="http://w3id.org/amlo/core#OriginatorParty">
        <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Parties/Parties/PartyInRole"/>
        <rdfs:comment xml:lang="it">identifica  un agente che ordina un trasferimento</rdfs:comment>
    </owl:Class>
    


    <!-- http://w3id.org/amlo/core#Reasoner -->

    <owl:Class rdf:about="http://w3id.org/amlo/core#Reasoner">
        <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Parties/Roles/ThingInRole"/>
        <rdfs:comment xml:lang="it">identifica un un agente software che ragionando su dati esistenti inferisce nuove informazioni</rdfs:comment>
    </owl:Class>
    


    <!-- http://w3id.org/amlo/core#RecipientBook -->

    <owl:Class rdf:about="http://w3id.org/amlo/core#RecipientBook">
        <rdfs:subClassOf rdf:resource="http://w3id.org/amlo/core#Book"/>
        <rdfs:comment xml:lang="it">identifica un registro  (es. libro mastro) usato  usato per annotare un trasferimento  dala parte del ricevente</rdfs:comment>
    </owl:Class>
    


    <!-- http://w3id.org/amlo/core#RecipientParty -->

    <owl:Class rdf:about="http://w3id.org/amlo/core#RecipientParty">
        <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Parties/Parties/PartyInRole"/>
        <rdfs:comment xml:lang="it">identifica un agente  come destinatario di un trasferimento</rdfs:comment>
    </owl:Class>
    


    <!-- http://w3id.org/amlo/core#RiskEvaluation -->

    <owl:Class rdf:about="http://w3id.org/amlo/core#RiskEvaluation">
        <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Arrangements/Assessments/Opinion"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/amlo/core#assessedOn"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/amlo/core#onRiskFactor"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="it">un opinione espressa su un rischio</rdfs:comment>
    </owl:Class>
    


    <!-- http://w3id.org/amlo/core#RiskRating -->

    <owl:Class rdf:about="http://w3id.org/amlo/core#RiskRating">
        <rdfs:subClassOf rdf:resource="http://w3id.org/amlo/core#RiskEvaluation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/amlo/core#hasRiskEstimator"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://w3id.org/amlo/core#RiskEstimator"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="it">una stima di un  fattore di rischio associato a qualcosa</rdfs:comment>
    </owl:Class>
    


    <!-- http://w3id.org/amlo/core#RiskSizing -->

    <owl:Class rdf:about="http://w3id.org/amlo/core#RiskSizing">
        <rdfs:subClassOf rdf:resource="http://w3id.org/amlo/core#RiskEvaluation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Quantities/QuantitiesAndUnits/hasQuantityValue"/>
                <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Quantities/QuantitiesAndUnits/QuantityValue"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="it">un dimensionamento del rischio basato su una o più quantità misurabili associato a qualcosa</rdfs:comment>
    </owl:Class>
    


    <!-- http://w3id.org/amlo/core#SuspectRating -->

    <owl:Class rdf:about="http://w3id.org/amlo/core#SuspectRating">
        <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Arrangements/Ratings/Rating"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/amlo/core#hasSuspectRatingScore"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://w3id.org/amlo/core#SuspectRatingScore"/>
