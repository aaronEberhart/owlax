<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY dcterms "http://purl.org/dc/terms/" >
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY swrl "http://www.w3.org/2003/11/swrl#" >
    <!ENTITY dc "http://purl.org/dc/elements/1.1/" >
    <!ENTITY swrlb "http://www.w3.org/2003/11/swrlb#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY owl2xml "http://www.w3.org/2006/12/owl2-xml#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY dqm "http://purl.org/dqm-vocabulary/v1/dqm#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY protege "http://protege.stanford.edu/plugins/owl/protege#" >
    <!ENTITY xsp "http://www.owl-ontologies.com/2005/08/07/xsp.owl#" >
]>


<rdf:RDF xmlns="http://purl.org/dqm-vocabulary/v1/dqm#"
     xml:base="http://purl.org/dqm-vocabulary/v1/dqm"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:protege="http://protege.stanford.edu/plugins/owl/protege#"
     xmlns:xsp="http://www.owl-ontologies.com/2005/08/07/xsp.owl#"
     xmlns:dqm="http://purl.org/dqm-vocabulary/v1/dqm#"
     xmlns:dcterms="http://purl.org/dc/terms/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:swrl="http://www.w3.org/2003/11/swrl#"
     xmlns:owl2xml="http://www.w3.org/2006/12/owl2-xml#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
    <owl:Ontology rdf:about="">
        <rdfs:label xml:lang="en"
            >The Data Quality Management Vocabulary - An Ontology for Data Requirements Management, Data Quality Monitoring, Data Quality Assessment, and Data Cleansing</rdfs:label>
        <dc:creator rdf:datatype="&xsd;string"
            >Christian F&#252;rber and Martin Hepp</dc:creator>
        <dc:subject rdf:datatype="&xsd;string"
            >Data Quality Management, Data Quality Assessment, Data Quality Monitoring, Data Profiling, Data Cleansing, Data Requirements Management</dc:subject>
        <dc:title rdf:datatype="&xsd;string"
            >The Data Quality Management Vocabulary</dc:title>
        <dc:rights rdf:datatype="&xsd;string"
            >The Data Quality Management Vocabulary is available under the Creative Commons Attribution - Non Commercial - Share Alike 3.0 Unported license; see http://creativecommons.org/licenses/by-nc-sa/3.0/. In a nutshell, you are free to copy, distribute and transmit the work and to remix/adapt the work (e.g. to import the ontology and create specializations of its elements) for non commercial purposes, as long as you attribute the work in the manner specified by the author or licensor (but not in any way that suggests that they endorse you or your use of the work). Proper Attribution: Simply include the statement &quot;This work is based on the Data Quality Management Vocabulary, developed by Christian F&#252;rber and Martin Hepp&quot; and link back to http://purl.org/dqm-vocabulary .</dc:rights>
        <owl:versionInfo rdf:datatype="&xsd;string"
            >V 1.00, Release 09-10-2011</owl:versionInfo>
        <dcterms:license rdf:datatype="&xsd;string"
            >http://creativecommons.org/licenses/by-nc-sa/3.0/</dcterms:license>
        <rdfs:comment xml:lang="en"
            >The Data Quality Management Vocabulary provides an ontology for the structured representation of data requirements, data quality assessment results, data cleansing rules, and data requirement violations connected to their origin. It, therefore, supports data quality monitoring, data quality assessment, and data cleansing in Semantic Web architectures.</rdfs:comment>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:AnnotationProperty rdf:about="&dc;title"/>
    <owl:AnnotationProperty rdf:about="&dcterms;license"/>
    <owl:AnnotationProperty rdf:about="&dc;contributor"/>
    <owl:AnnotationProperty rdf:about="&dc;creator"/>
    <owl:AnnotationProperty rdf:about="&dc;subject"/>
    <owl:AnnotationProperty rdf:about="&dc;rights"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dqm-vocabulary/v1/dqm#affectedClass -->

    <owl:ObjectProperty rdf:about="#affectedClass">
        <rdfs:label xml:lang="en">Affected Class</rdfs:label>
        <rdfs:comment xml:lang="en"
            >An affected class holds one or more instances with one or more data requirement violations.</rdfs:comment>
        <rdfs:range rdf:resource="#AffectedClass"/>
        <rdfs:domain rdf:resource="#DataRequirementViolation"/>
        <rdfs:subPropertyOf rdf:resource="#dataElementRelationship"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dqm-vocabulary/v1/dqm#affectedInstance -->

    <owl:ObjectProperty rdf:about="#affectedInstance">
        <rdfs:label xml:lang="de"
            >Affected Instance</rdfs:label>
        <rdfs:comment xml:lang="de"
            >An affected instance is the data instance that contains one or more data requirement violations.</rdfs:comment>
        <rdfs:range rdf:resource="#AffectedInstance"/>
        <rdfs:domain rdf:resource="#DataRequirementViolation"/>
        <rdfs:subPropertyOf rdf:resource="#dataElementRelationship"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dqm-vocabulary/v1/dqm#affectedProperty -->

    <owl:ObjectProperty rdf:about="#affectedProperty">
        <rdfs:label xml:lang="de"
            >Affected Property</rdfs:label>
        <rdfs:comment xml:lang="de"
            >An affected property specifies the property of a data instance that contains one or more data requirement violations. Some data requirement violations, such as functional dependency violations, have more than one affected property.</rdfs:comment>
        <rdfs:range rdf:resource="#AffectedProperty"/>
        <rdfs:domain rdf:resource="#DataRequirementViolation"/>
        <rdfs:subPropertyOf rdf:resource="#dataElementRelationship"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dqm-vocabulary/v1/dqm#appliesFor -->

    <owl:ObjectProperty rdf:about="#appliesFor">
        <rdfs:label xml:lang="en">Applise For</rdfs:label>
        <rdfs:comment xml:lang="en"
            >Connects data requirements with the tasks in which the requirement applies.</rdfs:comment>
        <rdfs:domain rdf:resource="#DataRequirement"/>
        <rdfs:range rdf:resource="#Task"/>
        <rdfs:subPropertyOf rdf:resource="#dataRequirementsRelationship"/>
        <owl:inverseOf rdf:resource="#hasRequirement"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dqm-vocabulary/v1/dqm#assessedClass -->

    <owl:ObjectProperty rdf:about="#assessedClass">
        <rdfs:label xml:lang="en">Assessed Class</rdfs:label>
        <rdfs:comment xml:lang="en"
            >The assessed class is the class that holds the instances that have been analyzed to compute the assessment score.</rdfs:comment>
        <rdfs:domain rdf:resource="#DataQualityScore"/>
        <rdfs:range rdf:resource="#TestedClass"/>
        <rdfs:subPropertyOf rdf:resource="#dataElementRelationship"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dqm-vocabulary/v1/dqm#assessedProperty -->

    <owl:ObjectProperty rdf:about="#assessedProperty">
        <rdfs:label xml:lang="en"
            >Assessed Property</rdfs:label>
        <rdfs:comment xml:lang="en"
            >The assessed property represents all properties that have been tested for rule violations during the computation of the assessment score.</rdfs:comment>
        <rdfs:domain rdf:resource="#DataQualityScore"/>
        <rdfs:range rdf:resource="#TestedProperty"/>
        <rdfs:subPropertyOf rdf:resource="#dataElementRelationship"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dqm-vocabulary/v1/dqm#basedOn -->

    <owl:ObjectProperty rdf:about="#basedOn">
        <rdfs:label xml:lang="en">Based On</rdfs:label>
        <rdfs:comment xml:lang="en"
            >Connects a data quality score with the data requirements that were used to calculate the score.</rdfs:comment>
        <rdfs:domain rdf:resource="#DataQualityScore"/>
        <rdfs:range rdf:resource="#DataRequirement"/>
        <rdfs:subPropertyOf rdf:resource="#dataRequirementsRelationship"/>
        <owl:inverseOf rdf:resource="#hasScore"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dqm-vocabulary/v1/dqm#blacklistClass -->

    <owl:ObjectProperty rdf:about="#blacklistClass">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">Blacklist Class</rdfs:label>
        <rdfs:comment xml:lang="en"
            >A blacklist class holds instances with values that are disallowed for a certain data set.</rdfs:comment>
        <rdfs:range rdf:resource="#BlacklistClass"/>
        <rdfs:domain rdf:resource="#IllegalValueRule"/>
        <rdfs:subPropertyOf rdf:resource="#dataElementRelationship"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dqm-vocabulary/v1/dqm#blacklistProperty -->

    <owl:ObjectProperty rdf:about="#blacklistProperty">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en"
            >Blacklist Property</rdfs:label>
        <rdfs:comment xml:lang="en"
            >A blacklist property holds the values that are disallowed for a certain data set.</rdfs:comment>
        <rdfs:range rdf:resource="#BlacklistProperty"/>
        <rdfs:domain rdf:resource="#IllegalValueRule"/>
        <rdfs:subPropertyOf rdf:resource="#dataElementRelationship"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dqm-vocabulary/v1/dqm#cleansesClass -->

    <owl:ObjectProperty rdf:about="#cleansesClass">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">Cleanses Class</rdfs:label>
        <rdfs:comment xml:lang="en"
            >This property identifies classes that contain instances that shall be cleansed via a data cleansing rule.</rdfs:comment>
        <rdfs:domain rdf:resource="#DataCleansingRule"/>
        <rdfs:range rdf:resource="#TestedClass"/>
        <rdfs:subPropertyOf rdf:resource="#dataCleansingRuleRelationship"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dqm-vocabulary/v1/dqm#cleansesProperty1 -->

    <owl:ObjectProperty rdf:about="#cleansesProperty1">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en"
            >Cleanses Property 1</rdfs:label>
        <rdfs:comment xml:lang="en"
            >This property identifies a property that holds values that shall be cleansed.</rdfs:comment>
        <rdfs:domain rdf:resource="#DataCleansingRule"/>
        <rdfs:range rdf:resource="#TestedProperty"/>
        <rdfs:subPropertyOf rdf:resource="#dataCleansingRuleRelationship"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dqm-vocabulary/v1/dqm#conditionalProperty -->

    <owl:ObjectProperty rdf:about="#conditionalProperty">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en"
            >Conditional Property</rdfs:label>
        <rdfs:comment xml:lang="en"
            >The conditional property property indicates the URI of a property that holds values that can be used to filter the relevant subset of data for which a rule applies.</rdfs:comment>
        <rdfs:domain rdf:resource="#Condition"/>
        <rdfs:range rdf:resource="#TestedProperty"/>
        <rdfs:subPropertyOf rdf:resource="#dataElementRelationship"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dqm-vocabulary/v1/dqm#dataCleansingRuleRelationship -->

    <owl:ObjectProperty rdf:about="#dataCleansingRuleRelationship">
        <rdfs:label xml:lang="en"
            >Data Cleansing Rule Relationship</rdfs:label>
        <rdfs:comment xml:lang="en"
            >Data cleansing rule relationships are object properties of the data cleansing rules.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dqm-vocabulary/v1/dqm#dataElementRelationship -->

    <owl:ObjectProperty rdf:about="#dataElementRelationship">
        <rdfs:label xml:lang="en"
            >Data Element Relationship</rdfs:label>
        <rdfs:comment xml:lang="en"
            >The data element relationship property is an abstract property that holds object properties that are used to define internal and external relationships of data elements.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dqm-vocabulary/v1/dqm#dataRequirementsRelationship -->

    <owl:ObjectProperty rdf:about="#dataRequirementsRelationship">
        <rdfs:label xml:lang="en"
            >Data Requirements Relationship</rdfs:label>
        <rdfs:comment xml:lang="en"
            >The data requirements relationship property is an abstract property that holds object properties that are used to define internal and external relationships of data requirements.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dqm-vocabulary/v1/dqm#hasCondition -->

    <owl:ObjectProperty rdf:about="#hasCondition">
        <rdfs:label xml:lang="en">Condition</rdfs:label>
        <rdfs:comment xml:lang="en"
            >Connects a conditional rule with a certain condition</rdfs:comment>
        <rdfs:range rdf:resource="#Condition"/>
        <rdfs:subPropertyOf rdf:resource="#dataElementRelationship"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dqm-vocabulary/v1/dqm#hasCondition1 -->

    <owl:ObjectProperty rdf:about="#hasCondition1">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">Has Condition 1</rdfs:label>
        <rdfs:comment xml:lang="en"
            >Connects a conditional rule with a certain condition.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="#hasCondition"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#ConditionalRule"/>
                    <rdf:Description rdf:about="#MultiPropertyCleansingRule"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dqm-vocabulary/v1/dqm#hasCondition2 -->

    <owl:ObjectProperty rdf:about="#hasCondition2">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">Has Condition 2</rdfs:label>
        <rdfs:comment xml:lang="en"
            >Connects a conditional rule with a certain condition.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="#hasCondition"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#ConditionalRule"/>
                    <rdf:Description rdf:about="#MultiPropertyCleansingRule"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dqm-vocabulary/v1/dqm#hasCondition3 -->

    <owl:ObjectProperty rdf:about="#hasCondition3">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">Has Condition 3</rdfs:label>
        <rdfs:comment xml:lang="en"
            >Connects a conditional rule with a certain condition.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="#hasCondition"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#ConditionalRule"/>
                    <rdf:Description rdf:about="#MultiPropertyCleansingRule"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dqm-vocabulary/v1/dqm#hasCondition4 -->

    <owl:ObjectProperty rdf:about="#hasCondition4">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">Has Condition 4</rdfs:label>
        <rdfs:comment xml:lang="en"
            >Connects a conditional rule with a certain condition.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="#hasCondition"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#ConditionalRule"/>
                    <rdf:Description rdf:about="#MultiPropertyCleansingRule"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dqm-vocabulary/v1/dqm#hasCondition5 -->

    <owl:ObjectProperty rdf:about="#hasCondition5">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">Has Condition 5</rdfs:label>
        <rdfs:comment xml:lang="en"
            >Connects a conditional rule with a certain condition.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="#hasCondition"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#ConditionalRule"/>
                    <rdf:Description rdf:about="#MultiPropertyCleansingRule"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dqm-vocabulary/v1/dqm#hasProperties -->

    <owl:ObjectProperty rdf:about="#hasProperties">
        <rdfs:label xml:lang="en">Has Properties</rdfs:label>
        <rdfs:comment xml:lang="en"
            >Reconnects mapped classes and properties with each other.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="#dataElementRelationship"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#AffectedProperty"/>
                    <rdf:Description rdf:about="#BlacklistProperty"/>
                    <rdf:Description rdf:about="#TestedProperty"/>
                    <rdf:Description rdf:about="#TrustedProperty"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#AffectedClass"/>
                    <rdf:Description rdf:about="#BlacklistClass"/>
                    <rdf:Description rdf:about="#TestedClass"/>
                    <rdf:Description rdf:about="#TrustedClass"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dqm-vocabulary/v1/dqm#hasRequirement -->

    <owl:ObjectProperty rdf:about="#hasRequirement">
        <rdfs:label xml:lang="en">Has Requirement</rdfs:label>
        <rdfs:comment xml:lang="en"
            >Specifies the data requirements that have to be fulfilled to perform the task.</rdfs:comment>
        <rdfs:range rdf:resource="#DataRequirement"/>
        <rdfs:domain rdf:resource="#Task"/>
        <rdfs:subPropertyOf rdf:resource="#dataRequirementsRelationship"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dqm-vocabulary/v1/dqm#hasScore -->

    <owl:ObjectProperty rdf:about="#hasScore">
        <rdfs:label xml:lang="en">Has Score</rdfs:label>
        <rdfs:comment xml:lang="en"
            >Connects data requirements with the data quality score which was calculated based on the requirement.</rdfs:comment>
        <rdfs:range rdf:resource="#DataQualityScore"/>
        <rdfs:domain rdf:resource="#DataRequirement"/>
        <rdfs:subPropertyOf rdf:resource="#dataRequirementsRelationship"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dqm-vocabulary/v1/dqm#next -->

    <owl:ObjectProperty rdf:about="#next">
        <rdfs:comment xml:lang="en"
            >Links an instances to the next instance in an ordinal scale.</rdfs:comment>
        <rdfs:domain rdf:resource="#ScaleValue"/>
        <rdfs:range rdf:resource="#ScaleValue"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dqm-vocabulary/v1/dqm#referenceClass -->

    <owl:ObjectProperty rdf:about="#referenceClass">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">Reference Class</rdfs:label>
        <rdfs:comment xml:lang="en"
            >The reference class property points to the URI of a class that contains trusted instances, i.e. instances of high quality.</rdfs:comment>
        <rdfs:range rdf:resource="#TrustedClass"/>
        <rdfs:subPropertyOf rdf:resource="#dataElementRelationship"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#FuncDepReferenceRule"/>
                    <rdf:Description rdf:about="#LegalValueRule"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dqm-vocabulary/v1/dqm#referenceProperty -->

    <owl:ObjectProperty rdf:about="#referenceProperty">
        <rdfs:label xml:lang="en"
            >Reference Property</rdfs:label>
        <rdfs:comment xml:lang="en"
            >The reference property points to the URI of a property that holds trusted values, i.e. values of high quality</rdfs:comment>
        <rdfs:range rdf:resource="#TrustedProperty"/>
        <rdfs:subPropertyOf rdf:resource="#dataElementRelationship"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dqm-vocabulary/v1/dqm#referenceProperty1 -->

    <owl:ObjectProperty rdf:about="#referenceProperty1">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en"
            >Reference Property 1</rdfs:label>
        <rdfs:comment xml:lang="en"
            >The reference property points to the URI of a property that holds trusted values, i.e. values of high quality</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="#referenceProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#FuncDepReferenceRule"/>
                    <rdf:Description rdf:about="#LegalValueRule"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dqm-vocabulary/v1/dqm#referenceProperty2 -->

    <owl:ObjectProperty rdf:about="#referenceProperty2">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en"
            >Reference Property 2</rdfs:label>
        <rdfs:comment xml:lang="en"
            >The reference property points to the URI of a property that holds trusted values, i.e. values of high quality</rdfs:comment>
        <rdfs:domain rdf:resource="#FuncDepReferenceRule"/>
        <rdfs:subPropertyOf rdf:resource="#referenceProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dqm-vocabulary/v1/dqm#referenceProperty3 -->

    <owl:ObjectProperty rdf:about="#referenceProperty3">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en"
            >Reference Property 3</rdfs:label>
        <rdfs:comment xml:lang="en"
            >The reference property points to the URI of a property that holds trusted values, i.e. values of high quality</rdfs:comment>
        <rdfs:domain rdf:resource="#FuncDepReferenceRule"/>
        <rdfs:subPropertyOf rdf:resource="#referenceProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dqm-vocabulary/v1/dqm#referenceProperty4 -->

    <owl:ObjectProperty rdf:about="#referenceProperty4">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en"
            >Reference Property 4</rdfs:label>
        <rdfs:comment xml:lang="en"
            >The reference property points to the URI of a property that holds trusted values, i.e. values of high quality</rdfs:comment>
        <rdfs:domain rdf:resource="#FuncDepReferenceRule"/>
        <rdfs:subPropertyOf rdf:resource="#referenceProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dqm-vocabulary/v1/dqm#referenceProperty5 -->

    <owl:ObjectProperty rdf:about="#referenceProperty5">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en"
            >Reference Property 5</rdfs:label>
        <rdfs:comment xml:lang="en"
            >The reference property points to the URI of a property that holds trusted values, i.e. values of high quality</rdfs:comment>
        <rdfs:domain rdf:resource="#FuncDepReferenceRule"/>
        <rdfs:subPropertyOf rdf:resource="#referenceProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dqm-vocabulary/v1/dqm#ruleOfIdentification -->

    <owl:ObjectProperty rdf:about="#ruleOfIdentification">
        <rdfs:label xml:lang="en"
            >Rule Of Identification</rdfs:label>
        <rdfs:comment xml:lang="en"
            >The rule of identification property connects the data quality problem instances with the instance of the data requirement that has detected the problem.</rdfs:comment>
        <rdfs:range rdf:resource="#DataRequirement"/>
        <rdfs:domain rdf:resource="#DataRequirementViolation"/>
        <rdfs:subPropertyOf rdf:resource="#dataRequirementsRelationship"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dqm-vocabulary/v1/dqm#ruleViolation -->

    <owl:ObjectProperty rdf:about="#ruleViolation">
        <rdfs:label xml:lang="en">Rule Violation</rdfs:label>
        <rdfs:comment xml:lang="en"
            >Connects data requirements with data quality problems that have been identified using the requirement.</rdfs:comment>
