topo:TypeDeZAI a owl:Class;
	rdfs:label "Type de ZAI"@fr;
	rdfs:label "Type of Area Of Interest (AOI)"@en;
	rdfs:comment "Regroupe l'ensemble des instances désignant des types de zones d'activités et d'intérêt."@fr, "Includes all instances designating types of activity and interest areas."@en;
	rdfs:isDefinedBy <http://data.ign.fr/def/topo>;
	rdfs:subClassOf skos:Concept.

topo:TypeDeBatiment a owl:Class;
	rdfs:label "Type de bâtiment"@fr;
	rdfs:label "Type of building "@en;
	rdfs:comment "Regroupe l'ensemble des instances désignant des types de bâtiments."@fr, "Includes all instances designating building types."@en;
	rdfs:isDefinedBy <http://data.ign.fr/def/topo>;
	rdfs:subClassOf skos:Concept.

topo:TypeDeConstruction a owl:Class;
	rdfs:label "Type de construction"@fr;
	rdfs:label "Type of structure"@en;
	rdfs:comment "Regroupe l'ensemble des instances désignant des types de constructions."@fr, "Group together all the instances designating types of constructions."@en;
	rdfs:isDefinedBy <http://data.ign.fr/def/topo>;
	rdfs:subClassOf skos:Concept.

topo:TypeDeReservoir a owl:Class;
	rdfs:label "Type de réservoir"@fr;
	rdfs:label "Type of tank"@en;
	rdfs:comment "Regroupe l'ensemble des instances désignant des types de réservoirs."@fr, "Group together all instances designating types of tanks."@en;
	rdfs:isDefinedBy <http://data.ign.fr/def/topo>;
	rdfs:subClassOf skos:Concept.

topo:TypeDeTerrainDeSport a owl:Class;
	rdfs:label "Type de terrain de sport"@fr;
	rdfs:label "Type of sports ground "@en;
	rdfs:comment "Regroupe l'ensemble des instances désignant des types de terrains de sports."@fr, "Includes all instances designating types of sports fields."@en;
	rdfs:isDefinedBy <http://data.ign.fr/def/topo>;
	rdfs:subClassOf skos:Concept.

topo:TypeDeVegetation a owl:Class;
	rdfs:label "Type de végétation"@fr;
	rdfs:label "Type of vegetation "@en;
	rdfs:comment "Regroupe l'ensemble des instances désignant des types de végétation arborée."@fr, "Includes all instances designating tree vegetation types."@en;
	rdfs:isDefinedBy <http://data.ign.fr/def/topo>;
	rdfs:subClassOf skos:Concept.

