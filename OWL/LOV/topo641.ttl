# -------------------------- SKOS ConceptSchemes definition -----------------------------------------------

zai:liste a skos:ConceptScheme;
	rdfs:comment "Liste des différents types de zones d'activité et d'intérêt, représentés comme une hiérarchie de concepts SKOS."@fr, "List of different types of activity areas and interests, represented as a hierarchy of SKOS concepts."@en;
	rdfs:label "Type de ZAI"@fr;
	rdfs:label "Type of AOI"@en;
	skos:hasTopConcept zai:PAIAdministratifOuMilitaire,	zai:PAICultureLoisirs, zai:PAIEspaceNaturel, zai:PAIScienceEnseignement, zai:PAIGestionEaux, zai:PAIIndustrielCommercial, zai:PAIReligieux, zai:PAISante, zai:PAISport, zai:PAITransport, zai:PAIZoneHabitation, zai:PAIHydrographie, zai:PAIOrographie.

bati:liste a skos:ConceptScheme;
	rdfs:comment "Liste des différents types de bâtiments, représentés comme une hiérarchie de concepts SKOS."@fr, "List of different types of buildings, represented as a hierarchy of SKOS concepts."@en;
	rdfs:label "Type de bâtiment"@fr;
	rdfs:label "Type of building"@en.

veg:liste a skos:ConceptScheme;
	rdfs:comment "Liste des différents types de végétation, représentés comme une hiérarchie de concepts SKOS."@fr, "List of different types of vegetation, represented as a hierarchy of SKOS concepts."@en;
	rdfs:label "Type de végétation"@fr;
	rdfs:label "Type of vegetation"@en;
	skos:hasTopConcept veg:ZoneArboree.

rel:liste a skos:ConceptScheme;
	rdfs:comment "Liste des différents types d'éléments du relief, représentés comme une hiérarchie de concepts SKOS."@fr, "List of different types of terrain elements, represented as a hierarchy of SKOS concepts."@en;
	rdfs:label "Type d'élément du relief"@fr;
	rdfs:label "Type of relief feature"@en.

cnstr:liste a skos:ConceptScheme;
	rdfs:comment "Liste des différents types de constructions, représentés comme une hiérarchie de concepts SKOS."@fr, "List of different types of constructions, represented as a hierarchy of SKOS concepts."@en;
	rdfs:label "Type de construction"@fr;
	rdfs:label "Type of structure"@en.

rsv:liste a skos:ConceptScheme;
	rdfs:comment "Liste des différents types de réservoirs, représentés comme une hiérarchie de concepts SKOS."@fr, "List of different types of tanks, represented as a hierarchy of SKOS concepts."@en;
	rdfs:label "Type de réservoirs"@fr;
	rdfs:label "Type of tank"@en.

tds:liste a skos:ConceptScheme;
	rdfs:comment "Liste des différents types de terrains de sport, représentés comme une hiérarchie de concepts SKOS."@fr, "List of different types of sports fields, represented as a hierarchy of SKOS concepts."@en;
	rdfs:label "Type de terrain de sport"@fr;
	rdfs:label "Type of sports ground"@en.

tpc:liste a skos:ConceptScheme;
	rdfs:comment "Liste des différents types lignes de transport par câble, représentés comme une hiérarchie de concepts SKOS."@fr, "List of different types of cable transport lines, represented as a hierarchy of SKOS concepts."@en;
	rdfs:label "Type de ligne de transport par câble"@fr;
	rdfs:label "Type of cableway transport line"@en.

vfr:liste a skos:ConceptScheme;
	rdfs:comment "Liste des différents types de voies ferrées, représentés comme une hiérarchie de concepts SKOS."@fr, "List of different types of railways, represented as a hierarchy of SKOS concepts."@en;
	rdfs:label "Type de voie ferrée"@fr;
	rdfs:label "Type of railway track"@en.

tdf:liste a skos:ConceptScheme;
	rdfs:comment "Liste des différents types d'obstacles ou d'infrastructures dédiées au franchissement d'obstacles, représentés comme une hiérarchie de concepts SKOS."@fr, "List of the different types of obstacle or infrastructure dedicated to obstacle clearance, represented as a hierarchy of SKOS concepts."@en;
	rdfs:label "Type de franchissement"@fr;
	rdfs:label "Type of crossing"@en.

rte:liste a skos:ConceptScheme;
	rdfs:comment "Liste des différents types de routes, représentés comme une hiérarchie de concepts SKOS."@fr, "List of different road types, represented as a hierarchy of SKOS concepts."@en;
	rdfs:label "Type de route"@fr;
	rdfs:label "Type of road"@en.

pte:liste a skos:ConceptScheme;
	rdfs:comment "Liste des différents types de points d'eau, représentés comme une hiérarchie de concepts SKOS."@fr, "List of different types of water points, represented as a hierarchy of SKOS concepts."@en;
	rdfs:label "Type de point d'eau"@fr;
	rdfs:label "Type of waterhole"@en.

tdl:liste a skos:ConceptScheme;
	rdfs:comment "Liste des différents types de laisses, représentés comme une hiérarchie de concepts SKOS."@fr, "List of different types of leashes, represented as a hierarchy of SKOS concepts."@en;
	rdfs:label "Type de laisse"@fr;
	rdfs:label "Type of tide line"@en.


# ------------------------------------------------ SKOS Concepts definition --------------------------------------------------

#-----------------------------------------------------------------
# ------ Concepts of the Scheme listeZAI -------
#-----------------------------------------------------------------
# ------ direct subclasses of TypeDeZAI -----

zai:PAIAdministratifOuMilitaire a topo:TypeDeZAI,
		skos:Concept;
	skos:definition "Désignation d'un établissement, site ou zone ayant un caractère public, administratif ou militaire."@fr, "Designation of an establishment, site or area of public, administrative or military character."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "PAI administratif ou militaire"@fr,"administrative or military POI"@en.

zai:PAICultureLoisirs a topo:TypeDeZAI,
		skos:Concept;
	skos:definition "Désignation d'un établissement ou lieu spécialement aménagé pour une activité culturelle, touristique ou de loisirs."@fr, "Designation of an establishment or place specially equipped for a cultural, tourist or leisure activity."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "PAI culture ou loisirs"@fr,"culture or leasure POI"@en.

zai:PAIEspaceNaturel a topo:TypeDeZAI,
		skos:Concept;
	skos:definition "Désignation d'un lieu-dit non habité dont le nom ne se rapporte ni à un détail orographique ni à un détail hydrographique."@fr, "Designation of an uninhabited locality whose name does not relate to an orographic detail or a hydrographic detail."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "PAI espace naturel"@fr,"natural POI"@en.

zai:PAIScienceEnseignement a topo:TypeDeZAI,
		skos:Concept;
	skos:definition "Désignation d'un établissement d'enseignement ou de recherche."@fr, "Designation of an educational or research institution."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "PAI science ou enseignement"@fr,"science or education POI"@en.

zai:PAIGestionEaux a topo:TypeDeZAI,
		skos:Concept;
	skos:definition "Désignation d'une construction ou site liés à l'approvisionnement, au traitement de l'eau pour différents besoins (agricole, industriel, consommation) ou à l'épuration des eaux usées avant rejet dans la nature."@fr, "Designation of a construction or site related to the supply, treatment of water for different needs (agricultural, industrial, consumption) or sewage treatment before release into the wild."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "PAI gestion des eaux"@fr,"water treatment POI"@en.

zai:PAIIndustrielCommercial a topo:TypeDeZAI,
		skos:Concept;
	skos:definition "Désignation d'un bâtiment, site ou zone à caractère industriel ou commercial."@fr, "Designation of a building, site or zone of an industrial or commercial nature."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "PAI industriel ou commercial"@fr,"industrial or commercial POI"@en.

zai:PAIReligieux a topo:TypeDeZAI,
		skos:Concept;
	skos:definition "Désignation d'un bâtiment réservé à la pratique d'une religion."@fr, "Designation of a building reserved for the practice of a religion."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "PAI religieux"@fr,"religious POI"@en.

zai:PAISante a topo:TypeDeZAI,
		skos:Concept;
	skos:definition "Désignation d'un établissement thermal ou de type hospitalier."@fr, "Designation of a thermal or hospital type establishment."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "PAI santé"@fr,"healthcare POI"@en.

zai:PAISport a topo:TypeDeZAI,
		skos:Concept;
	skos:definition "Désignation d'un établissement ou lieu spécialement aménagé pour la pratique d'une ou plusieurs activités sportives."@fr;
	skos:inScheme zai:liste;
	skos:prefLabel "PAI sport"@fr,"sports POI"@en.

zai:PAITransport a topo:TypeDeZAI,
		skos:Concept;
	skos:definition "Désignation d'un bâtiment ou site lié à une activité de transport routier, ferré ou aérien."@fr, "Designation of an establishment or place specially equipped for the practice of one or more sports activities."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "PAI transport"@fr,"transport POI"@en.

zai:PAIZoneHabitation a topo:TypeDeZAI,
		skos:Concept;
	skos:definition "Désignation d'un lieu-dit habité caractérisé par un nom."@fr, "Designation of an inhabited locality characterized by a name."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "PAI zone d'habitation"@fr,"dwellings zone POI"@en.

zai:PAIHydrographie a topo:TypeDeZAI,
		skos:Concept;
	skos:definition "Désignation se rapportant à un détail hydrographique."@fr, "Designation relating to a hydrographic detail."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "PAI hydrographie"@fr,"hydrographic POI"@en.
	
zai:PAIOrographie a topo:TypeDeZAI,
		skos:Concept;
	skos:definition "Désignation d'un détail du relief."@fr, "Designation of a relief detail."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "PAI orographie"@fr,"relief POI"@en.

#-----------------------------------------------------------------

zai:Borne a topo:TypeDeZAI, skos:Concept;
	skos:definition "Borne nommée : borne frontière, point de triangulation, point frontière."@fr, "Named terminal: boundary marker, triangulation point, boundary point."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "borne"@fr,"marker"@en;
	skos:broader zai:PAIAdministratifOuMilitaire.

zai:BureauDePoste a topo:TypeDeZAI, skos:Concept;
	skos:definition "Bureau de poste ouvert au public : Bureau de poste, hôtel des postes. Seuls les bureaux de poste ouverts en permanence sont inclus. En général, dans les agglomérations, seules les postes centrales sont incluses."@fr, "Post Office open to the public: Post Office, Post Office. Only permanently open post offices are included. In general, in the agglomerations, only the central stations are included."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "bureau de poste"@fr,"post office"@en;
	skos:altLabel "hôtel des postes"@fr,"general post office"@en;
	skos:broader zai:PAIAdministratifOuMilitaire.

zai:CaserneDePompiers a topo:TypeDeZAI, skos:Concept;
	skos:definition "Bâtiment ayant ou non un bureau ou une permanence et qui est entièment concerné par l'activité du corps des Sapeurs-Pompiers."@fr, "Building with or without an office or a permanence and which is entirely concerned by the activity of the body of the Fire-fighters."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "caserne de pompiers"@fr,"fire station"@en;
	skos:broader zai:PAIAdministratifOuMilitaire.

zai:EnceinteMilitaire a topo:TypeDeZAI, skos:Concept;
	skos:definition "Zone en permanence réservée pour les rassemblements de troupes de toutes les armes, soit pour des manoeuvres, des exercices (camp d'instruction), soit pour des essais, des études: base, camp, caserne, dépôt de matériels, terrain permanent d'entrainement, caserne de CRS, caserne de gendarmes mobiles, ... Les champs de tir sont exclus ainsi que les propriétés de l'armée qui ne sont indiquées d'aucune manière sur le terrain (ni clôtures, ni barrières, ni pancartes, ...) et ne faisant l'objet d'aucune restriction particulière."@fr, "Area permanently reserved for troop gatherings of all arms, either for maneuvers, exercises (training camp), or for tests, studies: base, camp, barracks, deposit of equipment, permanent ground of training, barracks of CRS, barracks of mobile gendarmes, ... The ranges are excluded as well as the properties of the army which are not indicated in any way on the ground (neither fences, barriers, nor signs,. ..) and not subject to any particular restriction."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "enceinte militaire"@fr,"military area"@en;
	skos:broader zai:PAIAdministratifOuMilitaire.

zai:EtablissementPenitenciaire a topo:TypeDeZAI, skos:Concept;
	skos:definition "Établissement clos aménagé pour recevoir des délinquants condamnés à une peine privative de liberté ou des détenus en instance de jugement: maison d'arrêt, prison. Les annexes sont exclues."@fr, "Enclosed institution equipped to receive offenders sentenced to deprivation of liberty or inmates awaiting trial: prison. The annexes are excluded."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "établissement pénitenciaire"@fr,"penitentiary"@en;
	skos:broader zai:PAIAdministratifOuMilitaire.

zai:Gendarmerie a topo:TypeDeZAI, skos:Concept;
	skos:definition "Caserne où les gendarmes sont logés ; bureaux où ils remplissent leurs fonctions administratives : gendarmerie, gendarmerie d'autoroute. Définition de l'emprise du site : surface de l'ensemble de la caserne, généralement délimitée par une clôture et incluant logements et bureaux."@fr, "Barracks where the gendarmes are lodged; offices where they fulfill their administrative functions: gendarmerie, highway police. Definition of the footprint of the site: area of the entire barracks, usually delimited by a fence and including housing and offices."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "gendarmerie"@fr,"gendarmerie"@en;
	skos:broader zai:PAIAdministratifOuMilitaire.

zai:HotelDeDepartement a topo:TypeDeZAI, skos:Concept;
	skos:definition "Bâtiment où siège le conseil général. Seul le bâtiment principal est inclus. Les annexes ne le sont pas, sauf éventuellement une annexe située dans une autre agglomération lorsqu'elle a une fonction proche de celle du siège."@fr, "Building where the General Council sits. Only the main building is included. The annexes are not, except possibly an annex located in another agglomeration when it has a function close to that of the headquarters."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "hôtel de département"@fr,"departmental administration building"@en;
	skos:broader zai:PAIAdministratifOuMilitaire.

zai:HotelDeRegion a topo:TypeDeZAI, skos:Concept;
	skos:definition "Bâtiment où siège le conseil régional. Seul le bâtiment principal est inclus. Les annexes ne le sont pas, sauf éventuellement une annexe située dans une autre agglomération lorsqu'elle a une fonction proche de celle du siège."@fr, "Building where the regional council sits. Only the main building is included. The annexes are not, except possibly an annex located in another agglomeration when it has a function close to that of the headquarters."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "hôtel de région"@fr,"regional administration building"@en;
	skos:broader zai:PAIAdministratifOuMilitaire.

zai:MaisonForestiere a topo:TypeDeZAI, skos:Concept;
	skos:definition "Maison gérée par l'office national des forêts. Les maisons de garde occupées par au moins un agent de l'ONF sont incluses. Les bureaux de l'ONF, les domiciles d'agents servant aussi de bureau, sont exclus lorsqu'ils ne sont pas situés dans une maison forestière."@fr, "House managed by the National Forest Office. Guard houses occupied by at least one NFB agent are included. The offices of the NFB, the homes of agents also used as offices, are excluded when they are not located in a forest house."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "maison forestière"@fr,"forest house"@en;
	skos:broader zai:PAIAdministratifOuMilitaire.

zai:OuvrageMilitaire a topo:TypeDeZAI, skos:Concept;
	skos:definition "Ouvrages ou installations militaires."@fr, "Military works or installations."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "ouvrage militaire"@fr,"defence work"@en;
	skos:broader zai:PAIAdministratifOuMilitaire.

zai:PalaisDeJustice a topo:TypeDeZAI, skos:Concept;
	skos:definition "Bâtiment où l'on rend la justice : palais de justice, tribunal. Seule la justice pénale est traitée. Les tribunaux administratifs sont exclus."@fr, "Building where justice is done: courthouse, court. Only criminal justice is treated. Administrative tribunals are excluded."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "palais de justice"@fr,"law-courts"@en;
	skos:broader zai:PAIAdministratifOuMilitaire.

zai:PosteDePolice a topo:TypeDeZAI, skos:Concept;
	skos:definition "Établissement occupé par un commissaire de police (officier de police judiciaire chargé de faire observer les règlements de police et de veiller au maintien de la paix publique) : hôtel de police nationale, commissariat, CRS d'autoroute, de port ou d'aéroport. Les bâtiments hébergeant uniquement la police municipale sont exclus. Les casernes de CRS et de gendarmes mobiles prennent la valeur 'enceinte militaire' et les gendarmeries la valeur 'gendarmerie'."@fr, "Establishment occupied by a police commissioner (judicial police officer responsible for enforcing police regulations and ensuring the maintenance of public peace): national police station, police station, highway, port or airport CRS. Buildings housing only the municipal police are excluded. The barracks of CRS and mobile gendarmes take the value 'military enclosure' and the gendarmeries the value 'gendarmerie'."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "poste de police"@fr,"police post"@en;
	skos:altLabel "hôtel de police"@fr,"police station"@en;
	skos:broader zai:PAIAdministratifOuMilitaire.

zai:PrefectureDeRegion a topo:TypeDeZAI, skos:Concept;
	skos:definition "Établissement qui abrite le siège de l'administration civile de la région. Les annexes sont exclues."@fr, "Establishment which houses the headquarters of the civil administration of the region. The annexes are excluded."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "préfecture de région"@fr,"regional prefecture"@en;
	skos:broader zai:PAIAdministratifOuMilitaire.

zai:Camping a topo:TypeDeZAI, skos:Concept;
	skos:definition "Emplacement aménagé pour la pratique du camping d'une superficie de plus de 2 ha."@fr, "Site arranged for the practice of the camping of an area of more than 2 ha."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "camping"@fr,"camping grounds"@en;
	skos:broader zai:PAICultureLoisirs.
	
zai:Construction a topo:TypeDeZAI, skos:Concept;
	skos:definition "Construction nommée habitée ou associée à un groupe d'habitations : construction diverse, pigeonnier, moulin à vent."@fr, "Construction named inhabited or associated with a group of houses: various construction, dovecote, windmill."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "construction nommée"@fr,"named building"@en;
	skos:broader zai:PAICultureLoisirs.
	
zai:Digue a topo:TypeDeZAI, skos:Concept;
	skos:definition "Digue, môle, jetée."@fr, "Dike, breakwater, jetty."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "digue"@fr,"dike"@en;
	skos:broader zai:PAICultureLoisirs.	
	
zai:Dolmen a topo:TypeDeZAI, skos:Concept;
	skos:definition "Monument mégalithique formé d'une grande pierre plate posée sur d'autres pierres dressées verticalement. Les allées couvertes sont incluses."@fr, "Megalithic monument formed by a large flat stone resting on other vertically standing stones. Covered walkways are included."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "dolmen"@fr,"dolmen"@en;
	skos:broader zai:PAICultureLoisirs.		
	
zai:EspacePublic a topo:TypeDeZAI, skos:Concept;
	skos:definition "Large espace découvert urbain désigné par un toponyme où aboutissent plusieurs rues, fermé à la circulation automobile, constituant un lieu remarquable : place, square, jardin, parc, parc communal, parc intercommunal, parc départemental, parc interdépartemental. Seuls les espaces publics possédant un toponyme sont retenus. Les parcs à vocation commerciale sont exclus (voir la valeur 'Parc de loisirs'), de même que les parcs naturels (réserves, parcs nationaux, parcs naturels régionaux) qui sont traités en PAI_ESPACE_NATUREL."@fr, "Large open space urban designated by a toponym where end several streets, closed to traffic, constituting a remarkable place: square, square, garden, park, communal park, intercommunal park, departmental park, interdepartmental park. Only public spaces with a toponym are retained. The commercial parks are excluded (see the value 'Leisure park'), as well as the natural parks (reserves, national parks, regional natural parks) which are treated in PAI_ESPACE_NATUREL."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "espace public nommé"@fr,"named public area"@en;
	skos:broader zai:PAICultureLoisirs.		
	
zai:HabitationTroglodytique a topo:TypeDeZAI, skos:Concept;
	skos:definition "Excavation naturelle ou creusée dans le roc (caverne, grotte), habitée ou anciennement habitée."@fr, "Natural excavation or excavated in the rock (cavern, cave), inhabited or formerly inhabited."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "habitation troglodytique"@fr,"cliff dwelling"@en;
	skos:broader zai:PAICultureLoisirs.	
	
zai:MaisonDuParc a topo:TypeDeZAI, skos:Concept;
	skos:definition "Bâtiment ouvert au public et géré par un Parc National ou Régional."@fr, "Building open to the public and managed by a National or Regional Park."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "maison du parc"@fr,"park center"@en;
	skos:broader zai:PAICultureLoisirs.		
	
zai:Menhir a topo:TypeDeZAI, skos:Concept;
	skos:definition "Pierre allongée, dressée verticalement. Les alignements en cromlech sont inclus."@fr, "Elongated stone, erected vertically. The alignments in cromlech are included."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "menhir"@fr,"menhir"@en;
	skos:broader zai:PAICultureLoisirs.	

zai:Musee a topo:TypeDeZAI, skos:Concept;
	skos:definition "Établissement ouvert au public exposant une grande collection d'objets, de documents, etc., relatifs aux arts et aux sciences et pouvant servir à leur histoire. Sont inclus : tous les musées controlés ou supervisés par le ministère de la Culture (musées nationaux, classés, controlés,etc.), les musées relevant de certains ministères techniques ou de l'assistance publique (musée de l'armée, de la marine), les musées privés ou associatifs ayant une grande notoriété, les écomusées."@fr, "Open to the public with a large collection of art and science objects, documents, etc. that can be used in their history. Included: all museums controlled or supervised by the Ministry of Culture (national museums, classified, controlled, etc.), museums under certain technical ministries or public assistance (Army Museum, Navy Museum ), private museums or associations with high notoriety, ecomuseums."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "musée"@fr,"museum"@en;
	skos:broader zai:PAICultureLoisirs.		

zai:ParcDesExpositions a topo:TypeDeZAI, skos:Concept;
	skos:definition "Lieu d'exposition ou de culture : centre culturel, parc des expositions."@fr, "Place of exhibition or culture: cultural center, exhibition center."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "parc des expositions"@fr,"fairground"@en;
	skos:broader zai:PAICultureLoisirs.	
	
zai:ParcDeLoisirs a topo:TypeDeZAI, skos:Concept;
	skos:definition "Parc à caractère commercial spécialement aménagé pour les loisirs : centre permanent de jeux, parc d'attractions, parc de détente, centre de loisirs. Seuls les parcs dont la superficie excède 10 ha et dotés d'équipements conséquents sont inclus. Les parcs publics (jardins, parcs communaux, départementaux) sont exclus (voir la valeur 'espace public')."@fr, "Commercial park specially designed for recreation: permanent games center, amusement park, relaxation park, leisure center. Only parks with an area exceeding 10 ha and equipped with substantial equipment are included. Public parks (gardens, municipal parks, departmental parks) are excluded (see the value 'public space')."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "parc de loisirs"@fr,"leisure park"@en;
	skos:broader zai:PAICultureLoisirs.	

zai:ParcZoologique a topo:TypeDeZAI, skos:Concept;
	skos:definition "Parc ouvert au public, où il est possible de voir des animaux sauvages vivant en captivité ou en semi-liberté. Tous les parcs ouverts au public sont inclus."@fr, "Park open to the public, where it is possible to see wild animals living in captivity or semi-freedom. All parks open to the public are included."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "parc zoologique"@fr,"zoological park"@en;
	skos:broader zai:PAICultureLoisirs.		
	
zai:Refuge a topo:TypeDeZAI, skos:Concept;
	skos:definition "Refuge, refuge gardé, abri de montagne nommé."@fr, "Refuge, sheltered refuge, named mountain shelter."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "refuge"@fr,"shelter"@en;
	skos:broader zai:PAICultureLoisirs.		
	
zai:Vestiges a topo:TypeDeZAI, skos:Concept;
	skos:definition "Vestiges archéologiques, fouilles, tumulus, oppidum."@fr, "Archaeological remains, excavations, burial mounds, oppidum."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "vestiges archéologiques"@fr,"archeological ruins"@en;
	skos:broader zai:PAICultureLoisirs.	
	
zai:VillageDeVacances a topo:TypeDeZAI, skos:Concept;
	skos:definition "Établissement de vacances comprenant des équipements sportifs ou de détente conséquents dont le gestionnaire est privé ou public: village de vacanaces, colonie de vacances. Les hôtels et les 'camps de vacances' sont exclus, ainsi que les établissements dont la capacité de rpise en charge est inférieure à 300 personnes."@fr, "Establishment of holidays including sports facilities or substantial relaxation of which the manager is private or public: village of vacanaces, colony of holidays. Hotels and 'holiday camps' are excluded, as well as establishments with a carrying capacity of less than 300 people."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "village de vacances"@fr,"holiday village"@en;
	skos:broader zai:PAICultureLoisirs.	
	
zai:Arbre a topo:TypeDeZAI, skos:Concept;
	skos:definition "Arbre nommé isolé, arbre remarquable."@fr, "Isolated named tree, remarkable tree."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "arbre"@fr,"tree"@en;
	skos:broader zai:PAIEspaceNaturel.		

zai:LieuDitNonHabite a topo:TypeDeZAI, skos:Concept;
	skos:definition "Lieu-dit quelconque, dont le nom est généralement attaché à des terres : lieu-dit non habité, plantation, espace cultivé."@fr, "Any locality, whose name is generally attached to land: uninhabited locality, plantation, cultivated area."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "lieu-dit non habité"@fr,"non inhabited local placename"@en;
	skos:broader zai:PAIEspaceNaturel.	

zai:Parc a topo:TypeDeZAI, skos:Concept;
	skos:definition "Espace réglementé, généralement libre d'accès pour le public et où la nature fait l'objet d'une protection spéciale : jardin, parc municipal, parc intercommunal, parc départemental, parc interdépartemental, parc naturel régional, parc national, réserve naturelle, parc marin. Les parcs à vocation commerciale ne sont pas pris en compte."@fr, "Regulated area, generally free for the public and where nature is the subject of special protection: garden, municipal park, intercommunal park, departmental park, interdepartmental park, regional nature park, national park, nature reserve, park marine. Commercial parks are not taken into account."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "parc"@fr,"park"@en;
	skos:broader zai:PAIEspaceNaturel.	

zai:PareFeu a topo:TypeDeZAI, skos:Concept;
	skos:definition "Dispositif destiné à empêcher la propagation d'un incendie (généralement, ouverture pratiquée dans le massif forestier menacé)."@fr, "Device designed to prevent the spread of a fire (usually opening in the endangered forest)."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "pare-feu"@fr,"fire-break"@en;
	skos:broader zai:PAIEspaceNaturel.

zai:PointDeVue a topo:TypeDeZAI, skos:Concept;
	skos:definition "Endroit d'où l'on jouit d'une vue pittoresque : point de vue, table d'orientation, belvédè. Seuls les points de vue aménagés (table d'orientation, bancs,etc.) sont inclus."@fr, "Place from where one enjoys a picturesque sight: point of view, orientation table, belvede. Only the points of view (orientation table, benches, etc.) are included."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "point de vue"@fr,"viewpoint"@en;
	skos:broader zai:PAIEspaceNaturel.	
	
zai:EnseignementPrimaire a topo:TypeDeZAI, skos:Concept;
	skos:definition "Établissement consacré à l'enseignement maternel et primaire : école primaire, école maternelle, groupe scolaire, Institut Médico-Pédagogique (I.M.P.). Tous les établissements d'enseignement primaire, publics, confessionnels ou privés, ayant un contrat simple ou d'association avec l'État sont inclus. Les crèches sont exclues."@fr, "Institution devoted to nursery and primary education: primary school, nursery school, school group, Institut Médico-Pédagogique (I.M.P.). All primary schools, public, religious or private, having a simple contract or association with the State are included. The nurseries are excluded."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "établissement d'enseignement maternel ou primaire"@fr,"nursery or elementary school"@en;
	skos:broader zai:PAIScienceEnseignement.	
	
zai:EnseignementSecondaire a topo:TypeDeZAI, skos:Concept;
	skos:definition "Établissement consacré à l'enseignement secondaire : collège, lycée, Centre d'Aide par le Travail (C.A.T.), Formation Professionnelle des Adultes (F.P.A.), Institut Médico-Professionnel (I.M.Pro.). Tous les établissements d'enseignement secondaire publics, confessionnels ou privés, ayant un contrat simple ou d'association avec l'État sont inclus."@fr, "Establishment devoted to secondary education: middle school, high school, Center for Help by Work (C.A.T.), Professional Training of Adults (F.P.A.), Medical-Professional Institute (I.M.Pro.). All public, religious or private secondary schools with a simple contract or association with the state are included."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "établissement d'enseignement secondaire"@fr,"secondary education school"@en;
	skos:broader zai:PAIScienceEnseignement.	

zai:EnseignementSuperieur a topo:TypeDeZAI, skos:Concept;
	skos:definition "Établissement consacré à l'enseignement supérieur : faculté, centre universitaire, institut, grande école,etc. Tous les établissements d'enseignement supérieur publics, confessionnels ou privés, ayant un contrat simple ou d'association avec l'État sont inclus. Les cours du soir, les cités et les restaurants universitaires sont exclus."@fr, "Institution devoted to higher education: faculty, university center, institute, grande école, etc. All public, religious or private higher education institutions with a simple contract or association with the state are included. Evening classes, cities and university restaurants are excluded."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "établissement d'enseignement supérieur"@fr,"postsecondary education school"@en;
	skos:broader zai:PAIScienceEnseignement.

zai:Science a topo:TypeDeZAI, skos:Concept;
	skos:definition "Établissement scientifique ou technique nommé: centre de recherche, laboratoire, observatoire, station scientifique."@fr, "Scientific or technical institution named: research center, laboratory, observatory, scientific station."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "établissement scientifique ou technique"@fr,"scientific or technological center"@en;
	skos:broader zai:PAIScienceEnseignement.	

zai:UsineDeTraitementDesEaux a topo:TypeDeZAI, skos:Concept;
	skos:definition "Établissement comprenant des installations destinées à rendre l'eau propre à la consommation (usine de traitement des eaux) ou à épurer des eaux usées avant leur rejet dans la nature (stations d'épuration, de lagunage) : usine de traitement des eaux, station d'épuration, station de lagunage. Les stations d'épuration et de lagunage sont incluses. Les stations traitant l'eau afin de la rendre propre à la consommation sont incluses lorsqu'elles comprennent des installations conséquentes (usines comprenant bassins, filtrages, traitements mécaniques). Sont exclues les stations lorsqu'il s'agit uniquement d'un traitement chimique d'appoint effectué au niveau d'un captage ou d'un réservoir. Les stations de relèvement sont également exclues."@fr, "Establishment with facilities to make water clean for consumption (water treatment plant) or to purify wastewater before discharge into the natural environment (sewage treatment plants, lagooning plants): water treatment plant, station purification plant, lagooning station. Treatment and lagooning plants are included. Stations treating water to make it fit for consumption are included when they include significant facilities (factories including basins, filtering, mechanical treatments). Stations are excluded when it concerns only a secondary chemical treatment carried out at the level of a catchment or a reservoir. Bearing stations are also excluded."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "usine de traitement des eaux"@fr,"water treatment plant"@en;
	skos:broader zai:PAIGestionEaux.

zai:Aquaculture a topo:TypeDeZAI, skos:Concept;
	skos:definition "Site aménagé pour l'élevage piscicole ou la culture d'espèces animales marines (coques, coquilles Saint-Jacques, huîtres, moules, palourdes,etc.) : bouchot, parc à huîtres, zone conchylicole, zone mytilicole, zone ostréicole. Toutes les zones de plus de 3 ha possédant des installations fixes de pêche et délimitées par des alignements de pieux, les parcs à huîtres, les bassins. Les installations de pêche au carrelet sont exclues."@fr, "Site developed for fish farming or the cultivation of marine animal species (hulls, scallops, oysters, mussels, clams, etc.): bouchot, oyster beds, shellfish growing area, mussel zone, oyster farming area. All areas of more than 3 ha with fixed fishing facilities and defined by pile alignments, oyster beds, ponds. The plaice fishing facilities are excluded."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "installation d'aquaculture"@fr,"aquaculture facilities"@en;
	skos:broader zai:PAIIndustrielCommercial.

zai:CentraleElectrique a topo:TypeDeZAI, skos:Concept;
	skos:definition "Usine où l'on produit de l'énergie électrique : centrale hydroélectrique, centrale thermique, centrale nucléaire. Les centrales électriques souterraines sont exclues."@fr, "Plant where electric power is produced: hydroelectric plant, thermal power station, nuclear power station. Underground power plants are excluded."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "centrale électrique"@fr,"power station"@en;
	skos:broader zai:PAIIndustrielCommercial.

zai:HarasNational a topo:TypeDeZAI, skos:Concept;
	skos:definition "Lieu ou établissement destiné à la reproduction de l'espèce chevaline, à l'amélioration des races de chevaux par la sélection des étalons. Tous les haras nationaux sont inclus. L'enceinte comprend l'ensemble des installations (manège, écuries, piste d'entraînement,etc.)."@fr, "Place or establishment intended for the reproduction of the horse species, for the improvement of the breeds of horses by the selection of the stallions. All national stud farms are included. The enclosure includes all the facilities (carousel, stables, training track, etc.)."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "haras national"@fr,"ranch"@en;
	skos:broader zai:PAIIndustrielCommercial.

zai:MaraisSalants a topo:TypeDeZAI, skos:Concept;
	skos:definition "Zone constituée de bassins creusés à proximité des côtes pour extraire le sel de l'eau de mer par évaporation. Les zones de marais salants de moins de 3 ha sont exclues. Les anciens marais salants qui ne sont plus en activité sont exclus."@fr, "Area consisting of basins dug near the coasts to extract salt from seawater by evaporation. Areas of salt marshes less than 3 ha are excluded. Old salt marshes that are no longer active are excluded."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "marais salants"@fr,"salt evaporator"@en;
	skos:broader zai:PAIIndustrielCommercial.
	
zai:Marche a topo:TypeDeZAI, skos:Concept;
	skos:definition "Tout ensemble construit dont la finalité est la commercialisation de gros ou de détail de denrées alimentaires : marché couvert, marché d'intérêt national, marché d'intérêt régional, halle, foire, zone d'exposition à caractère permanent, criée couverte."@fr, "Any building built for the purpose of marketing wholesale or retail foodstuffs: covered market, national interest market, regional interest market, hall, fair, permanent exhibition area, covered auction."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "marché"@fr,"market"@en;
	skos:broader zai:PAIIndustrielCommercial.	
	
zai:Mine a topo:TypeDeZAI, skos:Concept;
	skos:definition "Lieu d'où l'on extrait des minerais : mine de houille, mine de lignite, crassier, entrée de mine, terril. Les mines à ciel ouvert de plus de 10 ha sont incluses. Les mines souterraines sont exclues."@fr, "Location from which minerals are mined: coal mine, lignite mine, slag pit, mine entrance, slag heap. Open pit mines of more than 10 ha are included. Underground mines are excluded."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "mine"@fr,"mine"@en;
	skos:broader zai:PAIIndustrielCommercial.
	
zai:Usine a topo:TypeDeZAI, skos:Concept;
	skos:definition "Établissement dominé par une activité industrielle (fabrication d'objets ou de produits, transformation ou conservation de matières premières) : atelier, fabrique, manufacture, mine avec infrastructure bâtie, usine, scierie. Les sites dont la superficie est inférieure à 5 ha sont généralement exclus."@fr, "Establishment dominated by an industrial activity (manufacture of objects or products, transformation or conservation of raw materials): workshop, factory, manufacture, mine with built infrastructure, factory, sawmill. Sites with an area of less than 5 ha are generally excluded."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "usine"@fr,"factory"@en;
	skos:broader zai:PAIIndustrielCommercial.

zai:ZoneIndustrielle a topo:TypeDeZAI, skos:Concept;
	skos:definition "Regroupement d'activités de production sur l'initiative des collectivités locales ou d'organismes parapublics (chambres de commerce et d'industrie) et portant un nom : zone artisanale, zone industrielle. Les sites dont la superficie est inférieure à 5 ha sont généralement exclus."@fr, "Grouping of production activities on the initiative of local authorities or parastatals (chambers of commerce and industry) and bearing a name: artisanal zone, industrial zone. Sites with an area of less than 5 ha are generally excluded."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "zone industrielle"@fr,"industrial area"@en;
	skos:broader zai:PAIIndustrielCommercial.	
	
zai:Croix a topo:TypeDeZAI, skos:Concept;
	skos:definition "Monument religieux : croix, calvaire, vierge, statue religieuse."@fr, "Religious monument: cross, calvary, virgin, religious statue."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "croix"@fr,"cross"@en;
	skos:broader zai:PAIReligieux.
	
zai:CulteCatholiqueOuOrthodoxe a topo:TypeDeZAI, skos:Concept;
	skos:definition "Bâtiment réservé à l'exercice du culte catholique ou orthodoxe : église, cathédrale, basilique, chapelle, abbaye, oratoire."@fr, "Building reserved for the exercise of Catholic or Orthodox worship: church, cathedral, basilica, chapel, abbey, oratory."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "édifice religieux catholique ou orthodoxe"@fr,"catholic or orthodox building"@en;
	skos:broader zai:PAIReligieux.	

zai:CulteProtestant a topo:TypeDeZAI, skos:Concept;
	skos:definition "Bâtiment réservé à l'exercice du culte protestant : temple (protestant), église réformée."@fr, "Building reserved for the practice of Protestant worship: temple (Protestant), Reformed church."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "édifice religieux protestant"@fr,"protestant building"@en;
	skos:broader zai:PAIReligieux.
	
zai:CulteIsraelite a topo:TypeDeZAI, skos:Concept;
	skos:definition "Bâtiment réservé à l'exercice du culte israélite : synagogue."@fr, "Building reserved for the exercise of Jewish worship: synagogue."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "édifice religieux israélite"@fr,"jewish religious building"@en;
	skos:broader zai:PAIReligieux.	
	
zai:CulteIslamique a topo:TypeDeZAI, skos:Concept;
	skos:definition "Bâtiment réservé à l'exercice du culte islamique : mosquée."@fr, "Building reserved for the exercise of Islamic worship: mosque."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "édifice religieux islamique"@fr,"moslem religious building"@en;
	skos:broader zai:PAIReligieux.	

zai:CulteDivers a topo:TypeDeZAI, skos:Concept;
	skos:definition "Bâtiment réservé à l'exercice d'un culte religieux autre que chrétien, islamique ou israélite : temple bouddhiste, temple hindouiste."@fr, "Building reserved for religious worship other than Christian, Islamic or Jewish: Buddhist temple, Hindu temple."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "édifice religieux divers"@fr,"other religious building"@en;
	skos:broader zai:PAIReligieux.
	
zai:Tombeau a topo:TypeDeZAI, skos:Concept;
	skos:definition "Cimetière, tombe ou tombeau nommé : cimetière, tombe, tombeau, ossuaire, funérarium."@fr, "Cemetery, grave or tomb named: cemetery, tomb, tomb, ossuary, funeral parlor."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "tombeau"@fr,"tomb"@en;
	skos:broader zai:PAIReligieux.	

zai:EtablissementHospitalier a topo:TypeDeZAI, skos:Concept;
	skos:definition "Établissement public ou privé qui reçoit ou traite pendant un temps limité les malades, les blessés et les femmes en couches : hôpital, sanatorium, hospice, centre de soins, dispensaire, hôpital de jour, hôpital psychiatrique, etc. Tous les établissements assurant les soins et l'hébergement ou les soins seulement sont inclus. Les maisons de retraite ne possédant pas de centre de soins sont exclues."@fr, "Public or private institution that receives or deals for a limited time the sick, wounded and women in childbirth: hospital, sanatorium, hospice, health center, dispensary, day hospital, psychiatric hospital, etc. All facilities providing care and accommodation or care only are included. Retirement homes that do not have a health center are excluded."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "établissement hospitalier"@fr,"healthcare facility"@en;
	skos:broader zai:PAISante.

zai:EtablissementThermal a topo:TypeDeZAI, skos:Concept;
	skos:definition "Établissement où l'on utilise les eaux médicinales (eaux minérales, chaudes ou non) : établissement thermal, centre de thalassothérapie. Seuls sont inclus les établissements agréés par la Sécurité Sociale."@fr, "Establishment where medicinal waters are used (mineral waters, hot or not): thermal establishment, thalassotherapy center. Only establishments approved by the Social Security are included."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "établissement thermal"@fr,"health resort"@en;
	skos:broader zai:PAISante.

zai:Hopital a topo:TypeDeZAI, skos:Concept;
	skos:definition "Établissement public ou privé, où sont effectués tous les soins médicaux et chirurgicaux lourds et/ou de longue durée, ainsi que les accouchements : hôpital, CHU, hôpital militaire, clinique."@fr, "Public or private institution, where all the medical and surgical heavy and / or long-term care, as well as deliveries are carried out: hospital, university hospital, military hospital, clinic."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "hôpital"@fr,"hospital"@en;
	skos:broader zai:PAISante.
	
zai:Golf a topo:TypeDeZAI, skos:Concept;
	skos:definition "Terrain ouvert au public et consacré à la pratique du golf. Les terrains de moins de 9 trous et les minigolfs sont exclus."@fr, "Land open to the public and dedicated to the practice of golf. Less than 9 holes and minigolfs are excluded."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "golf"@fr,"golf course"@en;
	skos:broader zai:PAISport.	

zai:Hippodrome a topo:TypeDeZAI, skos:Concept;
	skos:definition "Lieu ouvert au public et consacré aux courses de chevaux. Seuls les hippodromes possédant des aménagements conséquents (tribunes, bâtiments spécifiques) sont inclus."@fr, "Open to the public and dedicated to horse racing. Only racetracks with significant amenities (stands, specific buildings) are included."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "hippodrome"@fr,"racecourse"@en;
	skos:broader zai:PAISport.

zai:Piscine a topo:TypeDeZAI, skos:Concept;
	skos:definition "Grand bassin de natation, et ensemble des installations qui l'entourent : piscine couverte, piscine découverte. Toutes les piscines ouvertes au public et ayant un bassin au moins de 25 m ou plus sont incluses. Les piscines des centres de vacances ou des hôtels sont exclues (voir la classe TERRAIN_SPORT)."@fr, "Large swimming pool, and all the facilities that surround it: indoor pool, outdoor pool. All pools open to the public and having a pool of at least 25 m or more are included. Pools at holiday centers or hotels are excluded (see the TERRAIN_SPORT class)."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "piscine"@fr,"swimming-pool"@en;
	skos:narrowMatch bati:BassinDeNatation;
	skos:broader zai:PAISport.

zai:Stade a topo:TypeDeZAI, skos:Concept;
	skos:definition "Grande enceinte, terrain aménagé pour la pratique des sports, et le plus souvent entouré de gradins, de tribunes : stade, terrain de sports, vélodrome découvert, circuit auto-moto, complexe sportif pluridisciplinaire. Seules les enceintes incluant des aménagements conséquents (piste 'construite', tribunes,etc.) sont incluses. Les terrains d'entraînement incluant seulement 2 ou 3 terrains de football et de petits vestiaires sont exclus (voir aussi la classe TERRAIN_SPORT)."@fr, "Large enclosure, ground arranged for the practice of the sports, and most often surrounded by bleachers, tribunes: stadium, ground of sports, velodrome discovered, circuit auto-moto, sport complex multidisciplinary. Only enclosures including substantial amenities (track 'built', stands, etc.) are included. Training grounds with only 2 or 3 football pitches and locker rooms are excluded (see also TERRAIN_SPORT class)."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "stade"@fr,"stadium"@en;
	skos:broader zai:PAISport.

zai:Aerodrome a topo:TypeDeZAI, skos:Concept;
	skos:definition "Tout terrain ou plan d'eau spécialement aménagé pour l'atterrissage, le décollage et les manoeuvres des aéronefs y compris les installations annexes qu'il peut comporter pour les besoins du trafic et le service des aéronefs : aérodrome militaire, héliport militaire.Ne sont pas pris en compte les aéro-clubs, les terrains de vol à voile, les pistes d'ULM."@fr, "Any land or body of water specially designed for the landing, take-off and maneuvering of aircraft, including ancillary facilities it may include for the purposes of traffic and the service of aircraft: military aerodrome, military heliport. not taken into account the flying clubs, the gliding grounds, the ULM tracks."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "aérodrome"@fr,"aerodrome"@en;
	skos:broader zai:PAITransport.

zai:AeroportInternational a topo:TypeDeZAI, skos:Concept;
	skos:definition "Aérodrome de statut international sur lequel ont été prévues des installations en vue de l'abri, de l'entretien ou de la répartition des aéronefs, ainsi que pour la réception, l'embarquement et le débarquement des passagers, le chargement et le déchargement des marchandises."@fr, "Aerodrome of international status on which facilities have been provided for the shelter, maintenance or distribution of aircraft, and for the reception, loading and unloading of passengers, the loading and unloading of merchandise."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "aéroport international"@fr,"international airport"@en;
	skos:broader zai:PAITransport.

zai:AeroportQuelconque a topo:TypeDeZAI, skos:Concept;
	skos:definition "Aérodrome sur lequel ont été prévues des installations en vue de l'abri, de l'entretien ou de la répartition des aéronefs, ainsi que pour la réception, l'embarquement et le débarquement des passagers, le chargement et le déchargement des marchandises."@fr, "Aerodrome on which facilities have been provided for the purpose of sheltering, maintaining or distributing aircraft, and for receiving, loading and unloading passengers, loading and unloading of goods."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "aéroport"@fr,"airport"@en;
	skos:broader zai:PAITransport.

zai:AireDeService a topo:TypeDeZAI, skos:Concept;
	skos:definition "Espace aménagé à l'écart des chaussées, notamment des autoroutes, pour permettre aux usagers de se ravitailler en carburant. Emprise de l'aire. Les contours de la surface ne s'appuient jamais sur des tronçons de route (qui représentent l'axe des chaussées)."@fr, "Space arranged away from roads, especially highways, to allow users to refuel. Emprise of the area. The contours of the surface never rest on sections of road (which represent the axis of pavements)."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "aire de service"@fr,"service area"@en;
	skos:broader zai:PAITransport.

zai:AireDeRepos a topo:TypeDeZAI, skos:Concept;
	skos:definition "Espace aménagé (présence d'un point d'eau obligatoire) à l 'écart des chaussées, notamment des autoroutes, pour permettre aux usagers de s'arrêter et de se reposer.Emprise de l'aire. Les contours de la surface ne s'appuient jamais sur des tronçons de route qui représentent l'axe des chaussées."@fr, "Developed space (presence of a compulsory water point) away from the roadways, especially highways, to allow users to stop and rest.Emprise of the area. The contours of the surface never rest on sections of road that represent the axis of pavements."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "aire de repos"@fr,"rest area"@en;
	skos:broader zai:PAITransport.

zai:Barrage a topo:TypeDeZAI, skos:Concept;
	skos:definition "Obstacle artificiel placé en travers d'un cours d'eau : barrage, écluse, vanne."@fr, "Artificial obstacle placed across a watercourse: dam, lock, valve."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "barrage"@fr,"barrage"@en;
	skos:narrowMatch bati:Barrage, bati:Ecluse;
	skos:broader zai:PAITransport.

zai:Carrefour a topo:TypeDeZAI, skos:Concept;
	skos:definition "Noeud du réseau routier : carrefour routier, échangeur, rond-point."@fr, "Node of the road network: road junction, interchange, roundabout."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "carrefour"@fr,"crossroads"@en;
	skos:broader zai:PAITransport.

zai:Chemin a topo:TypeDeZAI, skos:Concept;
	skos:definition "Voie de communication non routière : allée, chemin, laie forestière, sentier."@fr, "Non-road communication path: driveway, path, forest laie, path."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "chemin"@fr,"trail"@en;
	skos:broader zai:PAITransport.

zai:Echangeur a topo:TypeDeZAI, skos:Concept;
	skos:definition "Noeud du réseau routier à chaussées séparées. Échangeur autoroutier portant un nom."@fr, "Node of the divided road network. Motorway interchange bearing a name."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "échangeur"@fr,"interchange"@en;
	skos:broader zai:PAITransport.

zai:GareRoutiere a topo:TypeDeZAI, skos:Concept;
	skos:definition "Ensemble des installations destinées à l'embarquement et au débarquement de voyageurs en car ou en bus en un point déterminé. Ne sont pas retenues les gares routières des bus de ville, des bus scolaires, de la RATP et les dépôts de bus."@fr, "All facilities intended for the loading and unloading of passengers by bus or bus at a specific point. The bus stations of city buses, school buses, RATP and bus depots are not included."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "gare routière"@fr,"bus station"@en;
	skos:broader zai:PAITransport.
	
zai:GareVoyageursUniquement a topo:TypeDeZAI, skos:Concept;
	skos:definition "Établissement ferroviaire ou de transport par câble assurant avec ou sans personnel un service commercial de voyageurs : gare, station, point d'arrêt, station réseau ferré urbain, gare téléphérique. Toutes les gares et arrêts ferroviaires en service sont inclus."@fr, "Railway or cable transport establishment with or without a commercial passenger service: station, station, stopping point, urban railway station, cable car station. All stations and railway stops in use are included."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "gare de voyageurs"@fr,"passengers station"@en;
	skos:broader zai:PAITransport.	
	
zai:GareVoyageursEtFret a topo:TypeDeZAI, skos:Concept;
	skos:definition "Établissement ferroviaire assurant un service commercial de voyageurs et de marchandises.(Uniquement le bâtiment principal ouvert au public.)"@fr, "Railway establishment providing a commercial service for passengers and goods (Only the main building open to the public)"@en;
	skos:inScheme zai:liste;
	skos:prefLabel "gare de voyageurs et fret"@fr,"passengers and goods station"@en;
	skos:broader zai:PAITransport.	

zai:GareFretUniquement a topo:TypeDeZAI, skos:Concept;
	skos:definition "Établissement ferroviaire assurant un service commercial de marchandises : gare de fret, point de desserte. Le fret aérien ou maritime est exclu."@fr, "Railway establishment providing a commercial merchandise service: freight station, service point. Air or sea freight is excluded."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "gare de fret"@fr,"goods station"@en;
	skos:broader zai:PAITransport.

zai:InfrastructureRoutiere a topo:TypeDeZAI, skos:Concept;
	skos:definition "Élément du réseau routier ne figurant ni en attribut d'un objet 'tronçon de route' ni avec la valeur d'attribut 'carrefour' : route, aire de péage, aire de repos."@fr, "A road network element that is neither an attribute of a 'road segment' object nor the 'crossroad' attribute value: road, toll area, rest area."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "infrastructure routière"@fr,"road transport infrastructure"@en;
	skos:broader zai:PAITransport.

zai:Parking a topo:TypeDeZAI, skos:Concept;
	skos:definition "Une aire de stationnement ou parking est une zone aménagée pour le stationnement des véhicules : aire de stationnement, parking, parking souterrain, parking à étages. Tous les parkings publics nommés de plus de 25 places sont inclus qu'ils soient souterrains ou aériens (ex. parkings municipaux), Les parkings de plus de 25 places associés à des services de transport (gares, aéroports) sont retenus même s'ils n'ont pas de nom propre. Les parkings d'aires de repos ou de service ne sont pas retenus (voir les valeur 'Aire de repos' et 'Aire de service'). Le parkings appartenant à des établissements purement commerciaux (ex. parking de supermarché) sont exclus (pour ces derniers, voir aussi la classe 'Surface de route'). Un Parking est un objet ponctuel situé au centre de l'aire de stationnement, ou à l'entrée pour les parkings souterrains. Il est généralement associé à une surface pour des parkings aériens de plus de 5 ha."@fr, "A parking area or parking area is a parking area for vehicles: parking, parking, underground parking, parking. All public car parks with more than 25 spaces are included, whether underground or air (eg municipal car parks), Car parks with more than 25 spaces associated with transport services (train stations, airports) are retained even if they do not have a proper name. Parking spaces at rest or service areas are not retained (see 'Rest Area' and 'Service Area' values). Car parks belonging to purely commercial establishments (eg supermarket car park) are excluded (for these, see also the 'Road surface' class). Parking is a one-off object located in the center of the parking area, or at the entrance for underground car parks. It is generally associated with a surface area for car parks over 5 ha."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "parking"@fr,"car park"@en;
	skos:broader zai:PAITransport.
	
zai:BarriereDePeage a topo:TypeDeZAI, skos:Concept;
	skos:definition "Barrière de péage. Toutes les barrières de péage sont représentées, qu'elles soient ou non accompagnées d'un élargissement de la chaussée ou d'un bâtiment : péage d'autoroute, de pont, de route."@fr, "Toll barrier. All toll barriers are represented, whether or not accompanied by a widening of the roadway or a building: highway toll, bridge, road."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "barrière de péage"@fr,"toll-gate"@en;
	skos:relatedMatch bati:Peage;
	skos:broader zai:PAITransport.	

zai:Port a topo:TypeDeZAI, skos:Concept;
	skos:definition "Abri naturel ou artificiel aménagé pour recevoir les navires, pour l'embarquement et le débarquement de leur chargement : port de plaisance, port de pêche, port national, port privé, port international, port militaire."@fr, "Natural or artificial shelter fitted out to receive the ships, for loading and unloading of their load: marina, fishing port, national port, private port, international port, military port."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "port"@fr,"harbor"@en;
	skos:broader zai:PAITransport.

zai:RondPoint a topo:TypeDeZAI, skos:Concept;
	skos:definition "Rond-point, place de forme circulaire, ovale ou semi-circulaire, ou carrefour giratoire. Un giratoire est formé d'un anneau central qui permet aux usagers de prendre n'importe quelle direction, y compris de faire un demi-tour. Seuls les ronds-points nommés sont retenus."@fr, "Roundabout, circular, oval or semicircular square, or roundabout. A roundabout consists of a central ring that allows users to take any direction, including making a U-turn. Only the roundabouts named are retained."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "rond-point"@fr,"roundabout"@en;
	skos:broader zai:PAITransport.

zai:StationDeMetro a topo:TypeDeZAI, skos:Concept;
	skos:definition "Station où il est possible d'accéder au réseau du métro. On saisit un seul objet 'Station de métro' même s'il y a plusieurs entrées distinctes, éventuellement plusieurs ponctuels 'Station de métro' pour les correspondances importantes (ex : Bastille) mais un seul ponctuel pour une station de métro qui n'est pas une correspondance."@fr, "Station where it is possible to access the metro network. One seize only one object 'Metro Station' even if there are several separate entrances, possibly several punctual 'Metro Station' for important connections (eg Bastille) but only one punctual for a metro station that is not not a match."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "station de métro"@fr,"underground station"@en;
	skos:broader zai:PAITransport.

zai:VoieFerree a topo:TypeDeZAI, skos:Concept;
	skos:definition "Voie ferrée nommée"@fr, "Railroad name"@en;
	skos:inScheme zai:liste;
	skos:prefLabel "voie ferrée"@fr,"railway line"@en;
	skos:broader zai:PAITransport.

zai:Grange a topo:TypeDeZAI, skos:Concept;
	skos:definition "Construction légère : abri, baraquement, cabane, grange, hangar."@fr, "Light construction: shelter, barracks, hut, barn, shed."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "grange"@fr,"barn"@en;
	skos:broader zai:PAIZoneHabitation.

zai:LieuDitHabite a topo:TypeDeZAI, skos:Concept;
	skos:definition "Lieu-dit habité caractérisé par un nom."@fr, "Inhabited locality characterized by a name."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "lieu-dit habité"@fr,"inhabited local placename"@en;
	skos:broader zai:PAIZoneHabitation.

zai:Moulin a topo:TypeDeZAI, skos:Concept;
	skos:definition "Moulin ou ancien moulin à eau."@fr, "Mill or old water mill."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "moulin"@fr,"mill"@en;
	skos:broader zai:PAIZoneHabitation.

zai:Quartier a topo:TypeDeZAI, skos:Concept;
	skos:definition "Quartier nommé : cité, faubourg, lotissement."@fr, "Neighborhood named: city, suburb, housing estate."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "quartier"@fr,"district"@en;
	skos:broader zai:PAIZoneHabitation.

zai:Amer a topo:TypeDeZAI, skos:Concept;
	skos:definition "Point de repère visible de la mer : amer, bouée, balise, phare, feu, tourelle."@fr, "Visible landmark of the sea: bitter, buoy, beacon, lighthouse, fire, turret."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "amer"@fr,"seamark"@en;
	skos:broader zai:PAIHydrographie.

zai:Baie a topo:TypeDeZAI, skos:Concept;
	skos:definition "Espace marin pénétrant entre les terres : anse, baie, calanque, crique, golfe."@fr, "Marine space penetrating between the grounds: cove, bay, cove, cove, gulf."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "baie"@fr,"bay"@en;
	skos:broader zai:PAIHydrographie.

zai:Banc a topo:TypeDeZAI, skos:Concept;
	skos:definition "En mer ou sur un fleuve, relief sous-marin non rocheux représentant un danger potentiel pour la navigation: bancs, hauts-fonds."@fr, "At sea or on a river, non-rocky underwater relief representing a potential danger for navigation: benches, shoals."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "banc"@fr,"bank"@en;
	skos:broader zai:PAIHydrographie.
	
zai:Canal a topo:TypeDeZAI, skos:Concept;
	skos:definition "Cours d'eau artificiel : ancien canal, bief, canal, fossé, rigole."@fr, "Artificial water course: old canal, reach, canal, ditch, channel."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "canal"@fr,"channel"@en;
	skos:broader zai:PAIHydrographie.	
	
zai:Embouchure a topo:TypeDeZAI, skos:Concept;
	skos:definition "Embouchure d'un fleuve : delta, embouchure, estuaire."@fr, "Mouth of a river: delta, mouth, estuary."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "embouchure"@fr,"mouth"@en;
	skos:broader zai:PAIHydrographie.	

zai:EspaceMaritime a topo:TypeDeZAI, skos:Concept;
	skos:definition "Espace maritime, mer, océan, passe."@fr, "Maritime area, sea, ocean, pass."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "espace maritime"@fr,"maritime feature"@en;
	skos:broader zai:PAIHydrographie.
	
zai:Glacier a topo:TypeDeZAI, skos:Concept;
	skos:definition "Nom d'un glacier ou d'un détail relatif à un glacier : crevasse, glacier, moraine, névé, sérac."@fr, "Name of a glacier or a detail related to a glacier: crevasse, glacier, moraine, firn, serac."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "glacier"@fr,"glacier"@en;
	skos:broader zai:PAIHydrographie.	
	
zai:Lac a topo:TypeDeZAI, skos:Concept;
	skos:definition "Étendue d'eau terrestre : bassin, étang, lac, mare."@fr, "Body of water: pond, pond, lake, pond."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "lac"@fr,"lake"@en;
	skos:broader zai:PAIHydrographie.
	
zai:Marais a topo:TypeDeZAI, skos:Concept;
	skos:definition "Zone humide : marais, marécage, saline."@fr, "Wetland: marsh, swamp, saline."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "marais"@fr,"marsh"@en;
	skos:broader zai:PAIHydrographie.	
	
zai:Pecherie a topo:TypeDeZAI, skos:Concept;
	skos:definition "Zone d'activité aquacole : bouchot, parc à huîtres, pêcherie."@fr, "Aquaculture activity area: bouchot, oyster farm, fishery."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "pêcherie"@fr,"aquaculture area"@en;
	skos:broader zai:PAIHydrographie.		
	
zai:Perte a topo:TypeDeZAI, skos:Concept;
	skos:definition "Lieu où disparaît, où se perd un cours d'eau, qui réapparaît ensuite, en formant une résurgence, après avoir effectué un trajet souterrain."@fr, "Place where disappears, where a stream is lost, which reappears then, forming a resurgence, after having made an underground trip."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "perte"@fr,"loss"@en;
	skos:broader zai:PAIHydrographie.		
	
zai:PointDEau a topo:TypeDeZAI, skos:Concept;
	skos:definition "Tout point d'eau naturel ou artificiel : captage, citerne, fontaine, puits, résurgence, source."@fr, "Any natural or artificial water point: catchment, cistern, fountain, well, resurgence, source."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "point d'eau"@fr,"waterhole"@en;
	skos:broader zai:PAIHydrographie.	
	
zai:Riviere a topo:TypeDeZAI, skos:Concept;
	skos:definition "Cours d'eau: fleuve, rivière, ruisseau, torrent."@fr, "Stream: river, stream."@en;
	skos:inScheme zai:liste;
	skos:prefLabel "rivière"@fr,"river"@en;
	skos:broader zai:PAIHydrographie.	
	
		
	
#-----------------------------------------------------------------
# ------ Concepts of the Scheme TypeDeBatiment -------
#-----------------------------------------------------------------

bati:Aerogare a topo:TypeDeBatiment, skos:Concept;
	skos:definition "Ensemble des bâtiments d'un aéroport réservés aux voyageurs et aux marchandises."@fr, "Set of airport buildings reserved for travelers and goods."@en;
	skos:inScheme bati:liste;
	skos:prefLabel "aérogare"@fr,"air terminal"@en.	

bati:ArcDeTriomphe a topo:TypeDeBatiment, skos:Concept;
	skos:definition "Portique monumental : arc de triomphe, porte de ville."@fr, "Monumental portico: triumphal arch, city gate."@en;
	skos:inScheme bati:liste;
	skos:prefLabel "arc de triomphe"@fr,"triumphal arch"@en.	

bati:Arene a topo:TypeDeBatiment, skos:Concept;
	skos:definition "Vaste édifice à gradins, de forme en partie ou totalement ronde ou elliptique : amphithéâtre, arène, théâtre antique, théâtre de plein air."@fr, "Large tiered building, partly or totally round or elliptical in shape: amphitheater, arena, antique theater, outdoor theater."@en;
	skos:inScheme bati:liste;
	skos:prefLabel "arène"@fr,"arena"@en;
	skos:altLabel "théâtre antique"@fr, "roman theater"@en.

bati:BatimentReligieux a topo:TypeDeBatiment, skos:Concept;
	skos:definition "Bâtiment réservé à l’exercice d’un culte religieux, autre qu’une chapelle ou qu’une église : mosquée, synagogue, temple."@fr, "Building reserved for the exercise of a religious cult, other than a chapel or a church: mosque, synagogue, temple."@en;
	skos:inScheme bati:liste;
	skos:prefLabel "bâtiment religieux"@fr,"religious building"@en.	

bati:BatimentSportif a topo:TypeDeBatiment, skos:Concept;
	skos:definition "Bâtiment réservé à la pratique sportive : gymnase, piscine couverte, salle de sport, tennis couvert."@fr, "Sports building: gym, indoor pool, gym, indoor tennis."@en;
	skos:inScheme bati:liste;
	skos:prefLabel "bâtiment sportif"@fr,"sports building"@en.	

bati:Chapelle a topo:TypeDeBatiment, skos:Concept;
	skos:definition "Petit édifice religieux catholique de forme caractéristique."@fr, "Small Catholic religious building of characteristic form."@en;
	skos:inScheme bati:liste;
	skos:prefLabel "chapelle"@fr,"chapel"@en.	

bati:Chateau a topo:TypeDeBatiment, topo:TypeDeZAI, skos:Concept;
	skos:definition "Habitation ou ancienne habitation féodale, royale ou seigneuriale: château, château fort, citadelle, tour. Le lieu-dit, toujours nommé, peut ne pas être habité ou ne plus être habité mais n'est pas totalement en ruines."@fr, "Habitation or former feudal, royal or seigniorial dwelling: castle, castle, citadel, tower. The locality, still named, may not be inhabited or be inhabited but is not totally in ruins."@en;
	skos:inScheme bati:liste, zai:liste;
	skos:prefLabel "château"@fr,"castle"@en;
	skos:broader zai:PAIZoneHabitation.	
	
bati:Eglise a topo:TypeDeBatiment, skos:Concept;
	skos:definition "Édifice religieux catholique de forme caractéristique : basilique, cathédrale, église."@fr, "Characteristic Catholic religious building: basilica, cathedral, church."@en;
	skos:inScheme bati:liste;
	skos:prefLabel "église"@fr,"church"@en.

bati:Fort a topo:TypeDeBatiment, skos:Concept;
	skos:definition "Ouvrage militaire : blockhaus, casemate, fort, ouvrage fortifié."@fr, "Military work: blockhouse, casemate, fort, fortified work."@en;
	skos:inScheme bati:liste;
	skos:prefLabel "fort"@fr,"fort"@en;
	skos:altLabel "blockhaus"@fr, "blockhouse"@en;
	skos:altLabel "casemate"@fr, "casemate"@en.

bati:Gare a topo:TypeDeBatiment, skos:Concept;
	skos:definition "Bâtiment servant à l'embarquement et au débarquement des voyageurs en train."@fr, "Building used for boarding and disembarking passengers by train."@en;
	skos:inScheme bati:liste;
	skos:prefLabel "gare"@fr,"station"@en.

bati:Mairie a topo:TypeDeBatiment, skos:Concept;
	skos:definition "Édifice où se trouvent les services de l'administration municipale, appelé aussi hôtel de ville."@fr, "Building where are the services of the municipal administration, also called city hall."@en;
	skos:inScheme bati:liste, zai:liste;
	skos:prefLabel "mairie"@fr,"town-hall"@en;
	skos:altLabel "hôtel de ville"@fr, "city hall"@en;
	skos:broader zai:PAIAdministratifOuMilitaire.

bati:Monument a topo:TypeDeBatiment, skos:Concept;
	skos:definition "Monument commémoratif quelconque, sans caractère religieux particulier : monument, statue, stèle. Les arcs de triomphe sont exclus."@fr, "Any memorial without particular religious character: monument, statue, stele. The triumphal arches are excluded."@en;
	skos:inScheme bati:liste, zai:liste;
	skos:prefLabel "monument"@fr,"monument"@en;
	skos:broader zai:PAICultureLoisirs.
	
bati:Peage a topo:TypeDeBatiment, skos:Concept;
	skos:definition "Bâtiment où sont perçus les droits d‘usage."@fr, "Building where are collected the rights of use."@en;
	skos:inScheme bati:liste;
	skos:prefLabel "péage"@fr,"toll"@en;
	skos:relatedMatch zai:Peage.

bati:Prefecture a topo:TypeDeBatiment, skos:Concept;
	skos:definition "Bâtiment où sont installés les services préfectoraux."@fr, "Building where prefectural services are installed."@en;
	skos:inScheme bati:liste, zai:liste;
	skos:prefLabel "préfecture"@fr,"prefecture"@en;
	skos:broader zai:PAIAdministratifOuMilitaire.

bati:SousPrefecture a topo:TypeDeBatiment, skos:Concept;
	skos:definition "Bâtiment où sont les bureaux du sous-préfet: chef-lieu d’arrondissement."@fr, "Building where are the offices of the sub-prefect: chief town of district."@en;
	skos:inScheme bati:liste, zai:liste;
	skos:prefLabel "sous-préfecture"@fr,"subprefecture"@en;
	skos:broader zai:PAIAdministratifOuMilitaire.
	
bati:Tour a topo:TypeDeBatiment, skos:Concept;
	skos:definition "Bâtiment remarquable dans le paysage par sa forme élevée : donjon, moulin à vent, tour, tour de contrôle."@fr, "Remarkable building in the landscape by its high form: dungeon, windmill, tower, control tower."@en;
	skos:inScheme bati:liste;
	skos:prefLabel "tour"@fr,"tower"@en;
	skos:altLabel "donjon"@fr, "donjon"@en;
	skos:altLabel "moulin"@fr, "mill"@en.

bati:Tribune a topo:TypeDeBatiment, skos:Concept;
	skos:definition "Tribune de terrain de sport (stade, hippodrome, vélodrome,…)."@fr, "Tribune of sports ground (stadium, racecourse, velodrome, ...)."@en;
	skos:inScheme bati:liste;
	skos:prefLabel "tribune"@fr,"stand"@en.

bati:BatimentAgricole a topo:TypeDeBatiment, skos:Concept;
	skos:definition "Bâtiment réservé à des activités agricoles : bâtiment d'élevage industriel, hangar agricole (grand), minoterie."@fr, "Building reserved for agricultural activities: industrial breeding building, agricultural shed (large), flour mill."@en;
	skos:inScheme bati:liste;
	skos:prefLabel "bâtiment agricole"@fr,"farm building"@en.
	
bati:BatimentCommercial a topo:TypeDeZAI, topo:TypeDeBatiment, skos:Concept;
	skos:definition "Bâtiment ou zone de grande surface réservé à des activités commerciales : centre commercial, hypermarché, magasin (grand, isolé), parc des expositions (bâtiment), zone à caractère commercial."@fr, "Building or area of large area reserved for commercial activities: shopping center, hypermarket, store (large, isolated), fairground (building), commercial area."@en;
	skos:inScheme bati:liste, zai:liste;
	skos:prefLabel "bâtiment ou zone à caractère commercial"@fr,"commercial building or area"@en;
	skos:broader zai:PAIIndustrielCommercial.	
	
bati:BatimentIndustriel a topo:TypeDeBatiment, topo:TypeDeZAI, skos:Concept;
	skos:definition "Bâtiment ou zone réservé à des activités industrielles non distingué de façon spécifique : centre de recherche, dépôt, coopérative (vinicole, céréaliè'), champignonniè, élevage avicole, haras, abattoir, déchèterie, atelier (grand), auvent de quai de gare), auvent de péage, bâtiment industriel (grand), centrale électrique (bâtiment), construction technique, entrepôt, hangar industriel (grand), scierie, usine."@fr, "Building or area reserved for industrial activities not specifically distinguished: research center, depot, cooperative (winemaking, cereal '), mushroom, poultry farming, stud farm, slaughterhouse, refuse center, workshop (large), station platform awning) , toll canopy, industrial building (large), power station (building), technical construction, warehouse, industrial shed (big), sawmill, factory."@en;
	skos:inScheme bati:liste, zai:liste;
	skos:prefLabel "bâtiment ou zone à caractère industriel"@fr,"industrial building or area"@en;
	skos:broader zai:PAIIndustrielCommercial.	
	
bati:Serre a topo:TypeDeBatiment, skos:Concept;
	skos:definition "Abri clos à parois translucides destiné à protéger les végétaux du froid : jardinerie, serre."@fr, "Closed shelter with translucent walls intended to protect plants from the cold: gardening, greenhouse."@en;
	skos:inScheme bati:liste;
	skos:prefLabel "serre"@fr,"glass-house"@en.

bati:Silo a topo:TypeDeBatiment, skos:Concept;
	skos:definition "Réservoir, qui chargé par le haut se vide par le bas, et qui sert de dépôt, de magasin, etc. Le silo est exclusivement destiné aux produits agricoles : cuve à vin, silo."@fr, "Tank, which is loaded from the top empties from the bottom, and which serves as a depot, store, etc. The silo is exclusively for agricultural products: wine tank, silo."@en;
	skos:inScheme bati:liste;
	skos:prefLabel "silo"@fr,"silo"@en.

bati:ConstructionLegere a topo:TypeDeBatiment, skos:Concept;
	skos:definition "Structure légère non attachée au sol par l’intermédiaire de fondations (cabanes, abris de jardins…) ou bâtiment quelconque ouvert sur au moins un côté (préaux, auvents, tribunes)."@fr, "Light structure not attached to the ground through foundations (cabins, garden sheds ...) or any building open on at least one side (yards, awnings, stands)."@en;
	skos:inScheme bati:liste;
	skos:prefLabel "construction légère"@fr,"light construction"@en.

#-----------------------------------------------------------------
# ------ Concepts of the Scheme TypeDeReservoir -------
#-----------------------------------------------------------------

rsv:ChateauDEau a topo:TypeDeReservoir, skos:Concept;
	skos:definition "Réservoir d’eau construit en hauteur pour surélever le niveau de l’eau par rapport aux constructions."@fr, "Water tank built in height to raise the level of water compared to buildings."@en;
	skos:inScheme rsv:liste;
	skos:prefLabel "château d'eau"@fr,"water tower"@en.
	
rsv:ReservoirDEau a topo:TypeDeReservoir, skos:Concept;
	skos:definition "Réservoirs d’eau et châteaux d’eau au sol dans lesquels la réserve d’eau est située au niveau du sol."@fr, "Water tanks and ground water towers in which the water supply is located at ground level."@en;
	skos:inScheme rsv:liste;
	skos:prefLabel "réservoir d'eau"@fr,"water-tank"@en.

rsv:ReservoirIndustriel a topo:TypeDeReservoir, skos:Concept;
	skos:definition "Réservoir de matière première industrielle : gazomètre, réservoir d’hydrocarbure, réservoir de matériaux de construction, réservoir industriel."@fr, "Industrial raw material tank: gasometer, hydrocarbon tank, building material tank, industrial tank."@en;
	skos:inScheme rsv:liste;
	skos:prefLabel "réservoir industriel"@fr,"tank"@en.	
	
#-----------------------------------------------------------------
# ------ Concepts of the Scheme TypeDeTerrainDeSport -------
#-----------------------------------------------------------------

tds:PisteDeSport a topo:TypeDeTerrainDeSport, skos:Concept;
	skos:definition "Large piste en plein air réservée à la course : autodrome (piste), circuit automoto (piste), cynodrome (piste), hippodrome (piste), vélodrome (piste)."@fr, "Large outdoor track reserved for the race: autodrome (track), automoto circuit (track), track (racetrack), racecourse (track), velodrome (track)."@en; 
	skos:inScheme tds:liste;
	skos:prefLabel "piste de sport"@fr,"sports track"@en.

tds:TerrainDeTennis a topo:TypeDeTerrainDeSport, skos:Concept;
	skos:definition "Terrain spécialement aménagé pour la pratique du tennis."@fr, "Specially landscaped grounds for playing tennis."@en; 
	skos:inScheme tds:liste;
	skos:prefLabel "terrain de tennis"@fr,"tennis court"@en.
	
tds:BassinDeNatation a topo:TypeDeTerrainDeSport, skos:Concept;
	skos:definition "Bassin de natation d’une piscine découverte : bassin de natation, piscine (découverte)."@fr, "Swimming pool of an outdoor swimming pool: swimming pool, swimming pool (discovery)."@en; 
	skos:inScheme tds:liste;
	skos:prefLabel "bassin de natation"@fr,"swimming-pool"@en;
	skos:broadMatch zai:Piscine.

#-----------------------------------------------------------------
# ------ Concepts of the Scheme TypeDeConstruction -------
#-----------------------------------------------------------------

cnstr:Barrage a topo:TypeDeFranchissement, topo:TypeDeConstruction, skos:Concept;
	skos:definition "Ouvrage établi en travers du lit d’un cours d’eau et créant une dénivellation du plan d’eau entre l’amont et l’aval : barrage, vanne."@fr, "Work established across the bed of a river and creating a difference in level of water between upstream and downstream: dam, valve."@en; 
	skos:inScheme cnstr:liste, tdf:liste;
	skos:prefLabel "barrage"@fr,"dam"@en;
	skos:broadMatch zai:Barrage.

cnstr:MurAntiBruit a topo:TypeDeConstruction, skos:Concept;
	skos:definition "Mur destiné à protéger les riverains d’une voie de communication du bruit qu’elle engendre."@fr, "Wall intended to protect residents of a communication channel from the noise it generates."@en; 
	skos:inScheme cnstr:liste;
	skos:prefLabel "mur anti-bruit"@fr,"sound attenuation barrier"@en.

cnstr:Ruines a topo:TypeDeConstruction, skos:Concept;
	skos:definition "Bâtiment ou construction en ruines."@fr, "Building or construction in ruins."@en; 
	skos:inScheme cnstr:liste, zai:liste;
	skos:prefLabel "ruines"@fr,"ruins"@en;
	skos:broader zai:PAIZoneHabitation.

cnstr:Quai a topo:TypeDeConstruction, skos:Concept;
	skos:definition "Muraille en maçonnerie ou enrochement, élevée le long d’un cours d’eau pour retenir les berges, pour empêcher les débordements. Rivage d’un port aménagé pour l’accostage des bateaux. Ouvrage en maçonnerie s’opposant à l’écoulement des eaux marines ou fluviales."@fr, "Wall in masonry or rip rap, raised along a stream to retain the banks, to prevent overflows. Shoreline of a harbor arranged for the docking of the boats. Masonry structure opposing the flow of marine or fluvial waters."@en; 
	skos:inScheme cnstr:liste;
	skos:prefLabel "quai"@fr,"wharf"@en.

cnstr:Antenne a topo:TypeDeConstruction, skos:Concept;
	skos:definition "Construction destinée à la réception ou à l'émission d'ondes électromagnétiques : antenne, pylône de télécommunication."@fr, "Construction intended for receiving or emitting electromagnetic waves: antenna, telecommunication tower."@en; 
	skos:inScheme cnstr:liste;
	skos:prefLabel "antenne"@fr,"antenna"@en.

cnstr:Cheminee a topo:TypeDeConstruction, skos:Concept;
	skos:definition "Construction destinée à l'émission de fumées ou de gaz."@fr, "Construction intended for the emission of fumes or gas."@en; 
	skos:inScheme cnstr:liste;
	skos:prefLabel "cheminée"@fr,"chimney"@en.

cnstr:Eolienne a topo:TypeDeConstruction, skos:Concept;
	skos:definition "Une éolienne est un dispositif permettant d'utiliser l'énergie éolienne pour produire de l'énergie mécanique ou bien de l'électricité."@fr, "A wind turbine is a device that uses wind energy to produce mechanical energy or electricity."@en; 
	skos:inScheme cnstr:liste;
	skos:prefLabel "éolienne"@fr,"wind pump"@en.
	
cnstr:Phare a topo:TypeDeConstruction, skos:Concept;
	skos:definition "Tour élevée portant au sommet un foyer plus ou moins puissant destiné à guider les navires pendant la nuit."@fr, "High tower carrying at the top a more or less powerful focus intended to guide the ships during the night."@en; 
	skos:inScheme cnstr:liste;
	skos:prefLabel "phare"@fr,"lighthouse"@en.

cnstr:Torchere a topo:TypeDeConstruction, skos:Concept;
	skos:definition "Canalisation verticale par laquelle s'échappent et brûlent les résidus gazeux d'une raffinerie."@fr, "Vertical pipe through which escape and burn the gaseous residues of a refinery."@en; 
	skos:inScheme cnstr:liste;
	skos:prefLabel "torchère"@fr,"flare tower"@en.
	
cnstr:Transformateur a topo:TypeDeConstruction, skos:Concept;
	skos:definition "Bâtiment technique dont la fonction est de transformer un système de courants variables en un ou plusieurs autres systèmes de courants variables de même fréquence, mais d’intensité ou de tension généralement différentes."@fr, "Technical building whose function is to transform a system of variable currents into one or more other systems of variable currents of the same frequency, but of generally different intensity or voltage."@en; 
	skos:inScheme cnstr:liste;
	skos:prefLabel "transformateur"@fr,"transformer"@en.

cnstr:DalleDeProtection a topo:TypeDeConstruction, skos:Concept;
	skos:definition "Dalle (ou auvent) horizontale protégeant une voie de communication des chutes de pierres, des coulées de neige, ou protégeant le voisinage du bruit."@fr, "Horizontal slab (or awning) protecting a communication path from falling rocks, snowflows, or protecting the neighborhood from noise."@en; 
	skos:inScheme cnstr:liste;
	skos:prefLabel "dalle de protection"@fr,"protective wall"@en.

cnstr:Ecluse a topo:TypeDeConstruction, skos:Concept;
	skos:definition "Ouvrage hydraulique formé essentiellement de portes munies de vannes destiné à retenir ou à lâcher l’eau selon les besoins : ascenseur à bateaux, cale sèche, écluse, radoub."@fr, "Hydraulic structure consisting essentially of doors equipped with valves to retain or release water as needed: boat lift, dry dock, lock, refit."@en; 
	skos:inScheme cnstr:liste;
	skos:prefLabel "écluse"@fr,"lock"@en;
	skos:broadMatch zai:Barrage.
	
cnstr:EscalierMonumental a topo:TypeDeConstruction, skos:Concept;
	skos:definition "Escalier monumental."@fr, "Monumental staircase."@en; 
	skos:inScheme cnstr:liste;
	skos:prefLabel "escalier monumental"@fr,"monumental stairs"@en.
	

#-----------------------------------------------------------------
# ------ Concepts of the Scheme TypeDeVegetation -------
#-----------------------------------------------------------------
# ------ direct subclasses of TypeDeVegetation -----

veg:ZoneArboree a topo:TypeDeVegetation, skos:Concept;
	skos:definition "Espace peuplé d’arbres d’essence quelconque."@fr, "Space populated with trees of any kind."@en;
	skos:inScheme veg:liste;
	skos:prefLabel "zone arborée"@fr,"wooded area"@en.

# ------ subclasses of veg:ZoneArboree -----

veg:ForetFermeeFeuillus a topo:TypeDeVegetation, skos:Concept;
	skos:definition "Espace peuplé d’arbres forestiers couvrant au moins 40% du sol, et composée de plus de 75% de feuillus (taux de couvert relatif)."@fr, "Area populated with forest trees covering at least 40% of the soil, and composed of more than 75% of hardwoods (relative cover rate)."@en;
	skos:inScheme veg:liste;
	skos:prefLabel "forêt fermée de feuillus"@fr,"closed hardwood forest"@en;
	skos:broader bati:ZoneArboree.	

veg:ForetFermeeMixte a topo:TypeDeVegetation, skos:Concept;
	skos:definition "Espace peuplé d’arbres forestiers couvrant au moins 40% du sol, et composée de plus de 25% et moins de 75% de feuillus (taux de couvert relatif)."@fr, "Area populated with forest trees covering at least 40% of the soil, and composed of more than 25% and less than 75% of hardwoods (relative cover rate)."@en;
	skos:inScheme veg:liste;
	skos:prefLabel "forêt fermée mixte"@fr,"closed mixed forest"@en;
	skos:broader bati:ZoneArboree.

veg:ForetFermeeConiferes a topo:TypeDeVegetation, skos:Concept;
	skos:definition "Espace peuplé d’arbres forestiers couvrant au moins 40% du sol et composée essentiellement de conifères (soit un taux de couvert relatif de plus de 75%)."@fr, "Area populated with forest trees covering at least 40% of the soil and composed mainly of conifers (ie a relative cover rate of more than 75%)."@en;
	skos:inScheme veg:liste;
	skos:prefLabel "forêt fermée de conifères"@fr,"closed softwood forest"@en;
	skos:broader bati:ZoneArboree.

veg:ForetOuverte a topo:TypeDeVegetation, skos:Concept;
	skos:definition "Espace peuplé d’arbres forestiers dont le couvert absolu est compris entre 10% et 40%."@fr, "Area populated with forest trees whose absolute cover is between 10% and 40%."@en;
	skos:inScheme veg:liste;
	skos:prefLabel "forêt ouverte"@fr,"open forest"@en;
	skos:broader bati:ZoneArboree.

veg:Peupleraie a topo:TypeDeVegetation, skos:Concept;
	skos:definition "Zone plantée de peupliers."@fr, "Area planted with poplars."@en;
	skos:inScheme veg:liste;
	skos:prefLabel "peupleraie"@fr,"poplar plantation"@en;
	skos:broader bati:ZoneArboree.

veg:Haie a topo:TypeDeVegetation, skos:Concept;
	skos:definition "Clôture naturelle composée d’arbres, d’arbustes, d’épines ou de branchages et servant à limiter ou à protéger un champ. Alignement d’arbres ou plantation d’arbres fruitiers dont la largeur est inférieure à 25 m."@fr, "A natural fence made of trees, shrubs, thorns or branches that serves to limit or protect a field. Aligning trees or planting fruit trees less than 25 m wide."@en;
	skos:inScheme veg:liste;
	skos:prefLabel "haie"@fr,"hedge"@en;
	skos:broader bati:ZoneArboree.
	
veg:LandeLigneuse a topo:TypeDeVegetation, skos:Concept;
	skos:definition "Végétation spontanée qui comprend une proportion importante de plantes ligneuses (bruyères, genêts, ajoncs, épineux divers, etc.) et semi ligneuses (fougères, phragmites…) dont la hauteur ne dépasse pas 5 mètres. Cette végétation ligneuse présente un couvert végétal d’au moins 25%."@fr, "Spontaneous vegetation that includes a significant proportion of woody plants (heather, broom, gorse, various thorny, etc.) and semi woody (ferns, phragmites ...) whose height does not exceed 5 meters. This woody vegetation has a vegetative cover of at least 25%."@en;
	skos:inScheme veg:liste;
	skos:prefLabel "lande ligneuse"@fr,"woody heath"@en;
	skos:broader bati:ZoneArboree.	
	
veg:Verger a topo:TypeDeVegetation, skos:Concept;
	skos:definition "Zone de plantation d’arbres hors peuplement forestier."@fr, "Non-forest stand plantation area."@en;
	skos:inScheme veg:liste;
	skos:prefLabel "verger"@fr,"orchard"@en;
	skos:broader bati:ZoneArboree.

veg:Vigne a topo:TypeDeVegetation, skos:Concept;
	skos:definition "Zone de vignes."@fr, "Vineyard area."@en;
	skos:inScheme veg:liste;
	skos:prefLabel "vigne"@fr,"vineyard"@en;
	skos:broader bati:ZoneArboree.
	
veg:Bois a topo:TypeDeVegetation, skos:Concept;
	skos:definition "Espace peuplé d’arbres forestiers couvrant au moins 40% du sol."@fr, "Area populated by forest trees covering at least 40% of the soil."@en;
	skos:inScheme veg:liste, zai:liste;
	skos:prefLabel "bois"@fr,"wood"@en;
	skos:broader bati:ZoneArboree, zai:PAIEspaceNaturel.

veg:Bananeraie a topo:TypeDeVegetation, skos:Concept;
	skos:definition "Plantation de bananiers."@fr, "Banana plantation."@en;
	skos:inScheme veg:liste;
	skos:prefLabel "bananeraie"@fr,"banana plantation"@en;
	skos:broader bati:ZoneArboree.

veg:Mangrove a topo:TypeDeVegetation, skos:Concept;
	skos:definition "Dans les régions côtières intertropicales,formation végétale caractérisée par des forêts impénétrables de palétuviers qui fixent leurs fortes racines dans les baies aux eaux calmes où se déposent boues et limons."@fr, "In the intertropical coastal regions, vegetation formation characterized by impenetrable forests of mangroves that fix their strong roots in the bays with calm waters where sludge and silt are deposited."@en;
	skos:inScheme veg:liste;
	skos:prefLabel "mangrove"@fr,"mangrove forest"@en;
	skos:broader bati:ZoneArboree.

veg:CanneASucre a topo:TypeDeVegetation, skos:Concept;
	skos:definition "Plantation de canne à sucre."@fr, "Plantation of sugar cane."@en;
	skos:inScheme veg:liste;
	skos:prefLabel "canne à sucre"@fr,"sugarcane plantation"@en;
	skos:broader bati:ZoneArboree.

#-----------------------------------------------------------------
# ------ Concepts of the Scheme TypeDeRelief -------
#-----------------------------------------------------------------

rel:Levee a topo:TypeDeRelief, skos:Concept;
	skos:definition "Digue en terre (levée de terre) ou en maçonnerie de faible largeur : crassier, levée de terre, digue en terre, remblai, terril."@fr, "Dike in earth (raised earth) or in masonry of small width: slag, raised earth, earth dam, embankment, heap."@en;
	skos:inScheme rel:liste;
	skos:prefLabel "levée"@fr,"levee"@en.	
	
rel:MurDeSoutenement a topo:TypeDeRelief, skos:Concept;
	skos:definition "Mur épaulant un remblai, une terrasse."@fr, "Wall supporting an embankment, a terrace."@en;
	skos:inScheme rel:liste;
	skos:prefLabel "mur de soutènement"@fr,"retaining wall"@en.	
	
rel:Talus a topo:TypeDeRelief, skos:Concept;
	skos:definition "Ligne de rupture de pente : crassier, déblai, remblai, talus."@fr, "Slope breaking line: slag, excavation, embankment, embankment."@en;
	skos:inScheme rel:liste;
	skos:prefLabel "talus"@fr,"slope"@en.	

rel:Carriere a topo:TypeDeRelief, skos:Concept;
	skos:definition "Lieu d'où l'on extrait à ciel ouvert des matériaux de construction (pierre, roche) : carrière, sablière, ballastière, gravière. Grand talus marquant le front et la structure principale d’une carrière : gravière (talus), mine à ciel ouvert (talus), talus de carrière."@fr, "Place from where we extract construction materials (stone, rock): quarry, sandpit, ballast tank, gravel pit. Large embankment marking the front and main structure of a quarry: gravel pit (embankment), open pit mine (slope), quarry slope."@en;
	skos:inScheme rel:liste, zai:liste;
	skos:prefLabel "carrière"@fr,"quarry"@en;
	skos:broader zai:PAIIndustrielCommercial.		
	
rel:Cap a topo:TypeDeRelief, skos:Concept;
	skos:definition "Prédominance dans le contour d'une côte : cap, pointe, promontoire."@fr, "Predominance in the outline of a coast: cape, point, promontory."@en;
	skos:inScheme rel:liste, zai:liste;
	skos:prefLabel "cap"@fr,"cape"@en;
	skos:broader zai:PAIOrographie.	
	
rel:Cirque a topo:TypeDeRelief, skos:Concept;
	skos:definition "Dépression semi-circulaire, à bords raides."@fr, "Semi-circular depression, with steep edges."@en;
	skos:inScheme rel:liste, zai:liste;
	skos:prefLabel "cirque"@fr,"cirque"@en;
	skos:broader zai:PAIOrographie.	
	
rel:Col a topo:TypeDeRelief, skos:Concept;
	skos:definition "Point de passage imposé par la configuration du relief : col, passage."@fr, "Point of passage imposed by the relief configuration: pass, passage."@en;
	skos:inScheme rel:liste, zai:liste;
	skos:prefLabel "col"@fr,"mountain pass"@en;
	skos:broader zai:PAIOrographie.	
	
rel:Crete a topo:TypeDeRelief, skos:Concept;
	skos:definition "Ligne de partage des eaux : crête, arête, ligne de faîte."@fr, "Watershed: crest, ridge, ridge line."@en;
	skos:inScheme rel:liste, zai:liste;
	skos:prefLabel "crête"@fr,"crest"@en;
	skos:broader zai:PAIOrographie.	

rel:Depression a topo:TypeDeRelief, skos:Concept;
	skos:definition "Dépression naturelle du sol : cuvette, bassin fermé, dépression, doline."@fr, "Natural soil depression: basin, closed basin, depression, sinkhole."@en;
	skos:inScheme rel:liste, zai:liste;
	skos:prefLabel "dépression"@fr,"depression"@en;
	skos:broader zai:PAIOrographie.

rel:Dune a topo:TypeDeRelief, skos:Concept;
	skos:definition "Monticule de sable sur les bords de la mer."@fr, "Sand mound on the shores of the sea."@en;
	skos:inScheme rel:liste, zai:liste;
	skos:prefLabel "dune"@fr,"dune"@en;
	skos:broader zai:PAIOrographie.

rel:Escarpement a topo:TypeDeRelief, skos:Concept;
	skos:definition "Escarpement du relief : barre rocheuse, escarpement rocheux, face abrupte, falaise."@fr, "Relief of the relief: rocky ledge, rocky escarpment, abrupt face, cliff."@en;
	skos:inScheme rel:liste, zai:liste;
	skos:prefLabel "escarpement"@fr,"escarpment"@en;
	skos:broader zai:PAIOrographie.

rel:Gorge a topo:TypeDeRelief, skos:Concept;
	skos:definition "Vallée étroite et encaissée : canyon, cluse, défilé, gorge."@fr, "Valley narrow and concealed: canyon, cluse, parade, gorge."@en;
	skos:inScheme rel:liste, zai:liste;
	skos:prefLabel "gorge"@fr,"gorge"@en;
	skos:broader zai:PAIOrographie.
	
rel:Grotte a topo:TypeDeRelief, skos:Concept;
	skos:definition "Grotte naturelle ou excavation : aven, cave, gouffre, grotte."@fr, "Natural cave or excavation: aven, gulf, cave."@en;
	skos:inScheme rel:liste, zai:liste;
	skos:prefLabel "grotte"@fr,"cave"@en;
	skos:broader zai:PAIOrographie.
	
rel:Ile a topo:TypeDeRelief, skos:Concept;
	skos:definition "Île, îlot ou presqu'île."@fr, "Island or peninsula."@en;
	skos:inScheme rel:liste, zai:liste;
	skos:prefLabel "île"@fr,"island"@en;
	skos:broader zai:PAIOrographie.	
	
rel:Isthme a topo:TypeDeRelief, skos:Concept;
	skos:definition "Bande de terre étroite entre deux mers, réunissant deux terres : cordon littoral, isthme."@fr, "A narrow strip of land between two seas, uniting two lands: coastal cord, isthmus."@en;
	skos:inScheme rel:liste, zai:liste;
	skos:prefLabel "isthme"@fr,"isthmus"@en;
	skos:broader zai:PAIOrographie.	

rel:Montagne a topo:TypeDeRelief, skos:Concept;
	skos:definition "Désigne une montagne ou un massif de manière globale et non un sommet en particulier (voir sommet)."@fr, "Designates a mountain or a massif in a global way and not a particular summit (see summit)."@en;
	skos:inScheme rel:liste, zai:liste;
	skos:prefLabel "montagne"@fr,"mountain"@en;
	skos:broader zai:PAIOrographie.	

rel:Pic a topo:TypeDeRelief, skos:Concept;
	skos:definition "Sommet pointu d'une montagne : aiguille, pic, piton."@fr, "Sharp summit of a mountain: needle, peak."@en;
	skos:inScheme rel:liste, zai:liste;
	skos:prefLabel "pic"@fr,"peak"@en;
	skos:broader zai:PAIOrographie.	

rel:Plage a topo:TypeDeRelief, skos:Concept;
	skos:definition "Zone littorale marquée par le flux et le reflux des marées : grève, plage."@fr, "Coastal zone marked by the ebb and flow of the tides: beach."@en;
	skos:inScheme rel:liste, zai:liste;
	skos:prefLabel "plage"@fr,"beach"@en;
	skos:broader zai:PAIOrographie.	

rel:Plaine a topo:TypeDeRelief, skos:Concept;
	skos:definition "Zone de surface terrestre caractérisée par une relative planéité : plaine, plateau."@fr, "Land surface area characterized by relative flatness: plain, plateau."@en;
	skos:inScheme rel:liste, zai:liste;
	skos:prefLabel "plaine"@fr,"plain"@en;
	skos:broader zai:PAIOrographie.	

rel:Recif a topo:TypeDeRelief, skos:Concept;
	skos:definition "Rocher situé en mer ou dans un fleuve, mais dont une partie, faiblement émergée, peut constituer un obstacle ou un repère : brisant, récif, rocher marin."@fr, "Rock located in the sea or in a river, but part of which, slightly emerged, can constitute an obstacle or a landmark: breaking, reef, marine rock."@en;
	skos:inScheme rel:liste, zai:liste;
	skos:prefLabel "récif"@fr,"reef"@en;
	skos:broader zai:PAIOrographie.	

rel:Rochers a topo:TypeDeRelief, skos:Concept;
	skos:definition "Zone ou détail caractérisé par une nature rocheuse mais non verticale : chaos, éboulis, pierrier, rocher."@fr, "Zone or detail characterized by a rocky but not vertical nature: chaos, scree, rock."@en;
	skos:inScheme rel:liste, zai:liste;
	skos:prefLabel "rochers"@fr,"rocks"@en;
	skos:broader zai:PAIOrographie.	
	
rel:Sommet a topo:TypeDeRelief, skos:Concept;
	skos:definition "Point haut du relief non caractérisé par un profil abrupt (voir la nature Pic) : colline, mamelon, mont, sommet."@fr, "High point of the relief not characterized by an abrupt profile (see the nature Pic): hill, nipple, mount, summit."@en;
	skos:inScheme rel:liste, zai:liste;
	skos:prefLabel "sommet"@fr,"summit"@en;
	skos:broader zai:PAIOrographie.		
	
rel:Vallee a topo:TypeDeRelief, skos:Concept;
	skos:definition "Espace entre deux ou plusieurs montagnes. Forme définie par la convergence des versants et qui est, ou a été parcourue par un cours d'eau : combe, ravin, val, vallée, vallon, thalweg."@fr, "Space between two or more mountains. Form defined by the convergence of slopes and which is, or has been traversed by a river: combe, ravine, val, valley, thalweg."@en;
	skos:inScheme rel:liste, zai:liste;
	skos:prefLabel "vallée"@fr,"valley"@en;
	skos:broader zai:PAIOrographie.	
	
rel:Versant a topo:TypeDeRelief, skos:Concept;
	skos:definition "Plan incliné joignant une ligne de crête à un thalweg : coteau, versant."@fr, "Inclined plane joining a ridge line to a thalweg: hillside, slope."@en;
	skos:inScheme rel:liste, zai:liste;
	skos:prefLabel "versant"@fr,"slope"@en;
	skos:broader zai:PAIOrographie.		
	
rel:Volcan a topo:TypeDeRelief, skos:Concept;
	skos:definition "Toute forme de relief témoignant d'une activité volcanique : cratère, volcan."@fr, "Any form of relief testifying to volcanic activity: crater, volcano."@en;
	skos:inScheme rel:liste, zai:liste;
	skos:prefLabel "volcan"@fr,"volcano"@en;
	skos:broader zai:PAIOrographie.

#-----------------------------------------------------------------
# ------ Concepts of the Scheme TypeDeVoieFerree -------
#-----------------------------------------------------------------

vfr:LGV a topo:TypeDeVoieFerree, skos:Concept;
	skos:definition "Ligne ferroviaire réservée aux trains à grande vitesse."@fr, "Railway line reserved for high-speed trains."@en; 
	skos:inScheme vfr:liste;
	skos:prefLabel "ligne à grande vitesse"@fr,"ultra-high speed railway"@en.

vfr:VoiePrincipale a topo:TypeDeVoieFerree, skos:Concept;
	skos:definition "Voie ferrée en exploitation servant au transport des voyageurs ou des marchandises. RER, voie ferrée en exploitation, voie ferrée touristique, voie-mère d’embranchement particulier. Toutes les voies ferrées en exploitation assurant un service régulier ou saisonnier pour le transport des voyageurs ou des marchandises. Les lignes de métro et les lignes de TGV sont exclues."@fr, "Railway line in operation used for the transport of passengers or goods. RER, railway line in operation, tourist railway, special branch line. All operating rail lines providing regular or seasonal service for the transport of passengers or goods. Metro lines and TGV lines are excluded."@en; 
	skos:inScheme vfr:liste;
	skos:prefLabel "voie ferrée principale"@fr,"standard gauge railway"@en.

vfr:VoieDeService a topo:TypeDeVoieFerree, skos:Concept;
	skos:definition "Les voies de service de moins de 200 m de long sont exclues, ainsi que les voies s’inscrivant dans un faisceau de voies ferrées de plus de 25 m de large (voir <aire de triage>) et les voies de service souterraines."@fr, "Service roads of less than 200 m in length are excluded, as well as tracks within a railway line of more than 25 m wide (see <marshalling yard>) and underground service roads."@en; 
	skos:inScheme vfr:liste;
	skos:prefLabel "voie de service"@fr,"service track"@en.

vfr:VoieNonExploitee a topo:TypeDeVoieFerree, skos:Concept;
	skos:definition "Voie ferrée déclassée, voie ferrée fermée à tout trafic, voie ferrée neutralisée."@fr, "Decommissioned railway, railway closed to all traffic, neutralized railway."@en; 
	skos:inScheme vfr:liste;
	skos:prefLabel "voie non exploitée"@fr,"unused track"@en.

vfr:VoieTransportUrbain a topo:TypeDeVoieFerree, skos:Concept;
	skos:definition "Ligne de métro aérien ou de tramway."@fr, "Aerial metro or tram line."@en; 
	skos:inScheme vfr:liste;
	skos:prefLabel "voie de transport urbain"@fr,"urban transport railway"@en.

vfr:Funiculaire a topo:TypeDeVoieFerree, skos:Concept;
	skos:definition "Funiculaire ou voie ferrée à crémaillère."@fr, "Funicular or cog railway."@en; 
	skos:inScheme vfr:liste;
	skos:prefLabel "Funiculaire"@fr,"Funicular railway"@en;
	skos:altLabel "voie ferrée à crémaillère"@fr,"rack-railway"@en.

#-----------------------------------------------------------------
# ------ Concepts of the Scheme TypeDeTransportCable -------
#-----------------------------------------------------------------

tpc:CableTransporteur a topo:TypeDeTransportCable, skos:Concept;
	skos:definition "Moyen de transport constitué d'un ou de plusieurs câbles porteurs et servant à assurer le transport de matériel."@fr, "Means of transport consisting of one or more load-bearing cables used to transport equipment."@en; 
	skos:inScheme tpc:liste;
	skos:prefLabel "câble transporteur"@fr,"cableway"@en.

tpc:Telepherique a topo:TypeDeTransportCable, topo:TypeDeZAI, skos:Concept;
	skos:definition "Téléphériques, y compris les remonte-pentes, remontées mécaniques, télécabines, téléskis, télébennes, télésièges."@fr, "Cable cars, including ski lifts, lifts, gondola lifts, ski lifts, telebens, chair lifts."@en; 
	skos:inScheme tpc:liste, zai:liste;
	skos:prefLabel "téléphérique"@fr,"aerial ropeway"@en;
	skos:broader zai:PAITransport.

#-----------------------------------------------------------------
# ------ Concepts of the Scheme TypeDeFranchissement -------
#-----------------------------------------------------------------

tdf:Pont a topo:TypeDeFranchissement, skos:Concept;
	skos:definition "Ouvrage d’art susceptible d’enjamber un ou plusieurs éléments du réseau routier, ferré ou hydrographique ou permettant le franchissement d'une vallée : pont, passerelle, pont isolé, pont mobile. Portion du réseau située au dessus du niveau du sol."@fr, "A work of art likely to span one or more elements of the road, rail or hydrographic network or allowing the crossing of a valley: bridge, footbridge, isolated bridge, moving bridge. Portion of the network located above the ground level."@en; 
	skos:inScheme cnstr:liste, zai:liste, tdf:liste;
	skos:prefLabel "pont"@fr,"bridge"@en;
	skos:broader zai:PAITransport.

tdf:Tunnel a topo:TypeDeFranchissement, skos:Concept;
	skos:definition "Portion souterraine du réseau."@fr, "Underground portion of the network."@en; 
	skos:inScheme tdf:liste, zai:liste;
	skos:prefLabel "tunnel"@fr,"tunnel"@en;
	skos:broader zai:PAITransport.

tdf:Gue a topo:TypeDeFranchissement, skos:Concept;
	skos:definition "Passage naturel ou aménagé permettant aux véhicules de travaerser un cours d'eau sans le recours d'un pont ou d'un bateau."@fr, "Passage natural or arranged allowing vehicles to work a watercourse without the use of a bridge or a boat."@en; 
	skos:inScheme tdf:liste;
	skos:prefLabel "gué"@fr,"ford"@en.

tdf:Cascade a topo:TypeDeZAI, topo:TypeDeFranchissement, skos:Concept;
	skos:definition "Chute d’eau, cascade."@fr, "Waterfall"@en; 
	skos:inScheme tdf:liste, zai:liste;
	skos:prefLabel "cascade"@fr,"waterfall"@en;
	skos:broader zai:PAIHydrographie.

tdf:Ecluse a topo:TypeDeFranchissement, skos:Concept;
	skos:definition "Ouvrage hydraulique formé essentiellement de portes munies de vannes destiné à retenir ou à lâcher l’eau selon les besoins : Ascenseur à bateaux, écluse."@fr, "Hydraulic structure consisting essentially of doors equipped with valves to retain or release water as needed: Boat lift, lock."@en; 
	skos:inScheme tdf:liste;
	skos:prefLabel "écluse"@fr,"lock"@en.

tdf:PontCanal a topo:TypeDeFranchissement, skos:Concept;
	skos:definition "Tronçon de cours d’eau artificiel passant sur un pont."@fr, "Section of artificial watercourses passing over a bridge."@en; 
	skos:inScheme tdf:liste;
	skos:prefLabel "pont-canal"@fr,"canal aqueduct"@en.	

	
#-----------------------------------------------------------------
# ------ Concepts of the Scheme TypeDeRoute -------
#-----------------------------------------------------------------

rte:Autoroute a topo:TypeDeRoute, skos:Concept;
	skos:definition "Routes sans croisement, accessibles seulement en des points aménagés à cet effet et réservées aux véhicules à propulsion mécanique. Le classement dans la catégorie des autoroutes est prononcé par décret du conseil d’état, pris après enquête publique."@fr, "Roads without junction, accessible only at designated points for this purpose and reserved for vehicles with mechanical propulsion. The classification in the category of highways is pronounced by decree of the council of state, taken after public inquiry."@en; 
	skos:inScheme rte:liste;
	skos:prefLabel "autoroute"@fr,"motorway"@en.

rte:QuasiAutoroute a topo:TypeDeRoute, skos:Concept;
	skos:definition "Routes de même définition que l’autoroute mais non classées officiellement dans cette catégorie. Ce sont des routes à chaussées séparées par un terre-plein central, qui ne possèdent pas de croisement à niveau avec le reste du réseau routier."@fr, "Routes of the same definition as the highway but not officially classified in this category. These are paved roads separated by a median strip, which do not have a level crossing with the rest of the road network."@en; 
	skos:inScheme rte:liste;
	skos:prefLabel "quasi-autoroute"@fr,"non-classified motorway"@en.
	
rte:Bretelle a topo:TypeDeRoute, skos:Concept;
	skos:definition "Bretelles de liaison (ou d’échangeur) ou voies d’accès à une aire de service ou de repos."@fr, "Connecting braces (or interchange) or access routes to a service or rest area."@en; 
	skos:inScheme rte:liste;
	skos:prefLabel "bretelle"@fr,"ramp"@en.	

rte:RouteADeuxChaussees a topo:TypeDeRoute, skos:Concept;
	skos:definition "Routes comportant 2 chaussées séparées par un obstacle physique éventuellement ouvert aux carrefours. Elles possèdent donc des croisements à niveau, ce qui leur interdit d'être classées dans la catégorie autoroute ou quasi-autoroute."@fr, "Roads with 2 carriageways separated by a physical obstacle possibly open at intersections. They therefore have level crossings, which prevents them from being classified in the highway or near-highway category."@en; 
	skos:inScheme rte:liste;
	skos:prefLabel "route à deux chaussées"@fr,"dual carriageway road"@en.

rte:RouteAUneChaussee a topo:TypeDeRoute, skos:Concept;
	skos:definition "Routes comportant 1 chaussée. Toutes les routes goudronnées qui ne sont pas classées en Route à 2 chaussées, Quasi-autoroute ou Autoroute se retrouvent dans cette classe."@fr, "Roads with 1 roadway. All tarmac roads that are not classified as 2-lane Road, Near-highway or Highway are included in this class."@en; 
	skos:inScheme rte:liste;
	skos:prefLabel "route à une chaussée"@fr,"single carriageway road"@en.

rte:RouteEmpierree a topo:TypeDeRoute, skos:Concept;
	skos:definition "Routes sommairement revêtues (pas de revêtement de surface ou revêtement très dégradé), mais permettant la circulation de véhicules automobiles de tourisme par tout temps. Toutes les routes empierrées sont incluses."@fr, "Roads lightly surfaced (no surface coating or very degraded coating), but allowing the circulation of passenger cars all the time. All stone roads are included."@en; 
	skos:inScheme rte:liste;
	skos:prefLabel "route empierrée"@fr,"gravel road"@en.

rte:Chemin a topo:TypeDeRoute, skos:Concept;
	skos:definition "Les chemins sont prévus pour la circulation de véhicules ou d’engins d’exploitation. Ils ne sont pas forcément carrossables pour tous les véhicules et par tout temps."@fr, "Roads are intended for the circulation of vehicles or machinery. They are not necessarily drivable for all vehicles and in any weather."@en; 
	skos:inScheme rte:liste;
	skos:prefLabel "chemin"@fr,"dirt road"@en.

rte:BacAuto a topo:TypeDeRoute, skos:Concept;
	skos:definition "Trajets du bateau servant à passer des véhicules d’une rive à l’autre d’un cours d’eau ou d’un bras de mer."@fr, "Routes of the boat used to pass vehicles from one bank to the other of a watercourse or an arm of the sea."@en; 
	skos:inScheme rte:liste;
	skos:prefLabel "bac auto"@fr,"car ferry"@en.

rte:BacPieton a topo:TypeDeRoute, skos:Concept;
	skos:definition "Trajets du bateau servant à passer des piétons d’une rive à l’autre d’un cours d’eau ou d’un bras de mer."@fr, "Routes of the boat used to cross pedestrians from one bank to the other of a watercourse or an arm of the sea."@en; 
	skos:inScheme rte:liste;
	skos:prefLabel "bac piéton"@fr,"passenger ferry"@en.

rte:PisteCyclable a topo:TypeDeRoute, skos:Concept;
	skos:definition "Tronçons de chaussée en site propre, réservés aux cycles. Les bandes cyclables sont exclues."@fr, "Segments of pavement in own site, reserved for bicycles. Bike lanes are excluded."@en; 
	skos:inScheme rte:liste;
	skos:prefLabel "piste cyclable"@fr,"bicycle path"@en.
	
rte:Sentier a topo:TypeDeRoute, skos:Concept;
	skos:definition "Chemins étroits ne permettant pas le passage de véhicules. Seuls les principaux sentiers sont inclus. Passerelles supportant une allée, directement reliés au réseau routier. Les passerelles ont une position par rapport au sol supérieure à 0."@fr, "Narrow paths that do not allow the passage of vehicles. Only the main trails are included. Bridges supporting an alley, directly connected to the road network. The bridges have a position relative to the ground greater than 0."@en; 
	skos:inScheme rte:liste;
	skos:prefLabel "sentier"@fr,"footpath"@en.	

rte:Escalier a topo:TypeDeRoute, skos:Concept;
	skos:definition "Escaliers directement reliés au réseau routier, assurant la jonction entre deux voies de communication ou entre le réseau routier et un élément adressable."@fr, "Stairs directly connected to the road network, ensuring the junction between two communication channels or between the road network and an addressable element."@en; 
	skos:inScheme rte:liste;
	skos:prefLabel "escalier"@fr,"stair way street"@en.	

rte:RouteNationale a topo:TypeDeRoute, skos:Concept;
	skos:definition "Voie du domaine public routier national autre que les autoroutes."@fr, "National public highway roadway other than motorways."@en; 
	skos:inScheme rte:liste;
	skos:prefLabel "route nationale"@fr,"main road"@en.	

rte:RouteDepartementale a topo:TypeDeRoute, skos:Concept;
	skos:definition "Voie qui fait partie du domaine public routier départemental."@fr, "Road that is part of the public road departmental."@en; 
	skos:inScheme rte:liste;
	skos:prefLabel "route départementale"@fr,"secondary road"@en.

#-----------------------------------------------------------------
# ------ Concepts of the Scheme TypeDePointDEau -------
#-----------------------------------------------------------------

pte:Citerne a topo:TypeDePointDEau, skos:Concept;
	skos:definition "Réservoir où l'on recueille et conserve les eaux de pluie."@fr, "Tank where rainwater is collected and stored."@en; 
	skos:inScheme pte:liste;
	skos:prefLabel "citerne"@fr,"cistern"@en.

pte:Fontaine a topo:TypeDePointDEau, skos:Concept;
	skos:definition "Construction comportant une alimentation en eau et généralement un bassin."@fr, "Construction comprising a water supply and generally a pond."@en; 
	skos:inScheme pte:liste;
	skos:prefLabel "fontaine"@fr,"fountain"@en.

pte:Source a topo:TypeDePointDEau, skos:Concept;
	skos:definition "Point d'émergence à la surface du sol de l'eau emmagasinée à l'intérieur. Exsurgence, résurgence, source."@fr, "Point of emergence on the soil surface of the water stored inside. Exsurgence, resurgence, source."@en; 
	skos:inScheme pte:liste;
	skos:prefLabel "source"@fr,"spring"@en.

pte:SourceCaptee a topo:TypeDePointDEau, skos:Concept;
	skos:definition "Installation de captage d’une source."@fr, "Collection installation of a source."@en; 
	skos:inScheme pte:liste;
	skos:prefLabel "source captée"@fr,"water catchment"@en.

pte:StationDePompage a topo:TypeDePointDEau, topo:TypeDeZAI, skos:Concept;
	skos:definition "Installation comprenant une pompe destinée à pomper l'eau du sous-sol, d'une rivière, ou à monter de l'eau dans un réservoir ou un château d'eau."@fr, "Installation comprising a pump for pumping water from the basement, a river, or to raise water in a tank or a water tower."@en; 
	skos:inScheme pte:liste, zai:liste;
	skos:prefLabel "station de pompage"@fr,"pumping station"@en;
	skos:broader zai:PAIGestionEaux.


#-----------------------------------------------------------------
# ------ Concepts of the Scheme TypeDeLaisse -------
#-----------------------------------------------------------------

tdl:HautesMers a topo:TypeDeLaisse, skos:Concept;
	skos:definition "Limite supérieure de l'estran. Elle sépare la zone littorale découvrante de la zone jamais envahie par la mer."@fr, "Upper limit of the foreshore. It separates the discovering littoral zone from the zone never invaded by the sea."@en; 
	skos:inScheme tdl:liste;
	skos:prefLabel "laisse des plus hautes mers"@fr,"high-tide line"@en.

tdl:BassesMers a topo:TypeDeLaisse, skos:Concept;
	skos:definition "Limite inférieure de l'estran. Elle sépare la zone littorale découvrante de la zone toujours envahie par la mer."@fr, "Lower limit of the foreshore. It separates the discovering littoral zone from the zone always invaded by the sea."@en; 
	skos:inScheme tdl:liste;
	skos:prefLabel "laisse des plus basses mers"@fr,"low-tide line"@en.
	
	
