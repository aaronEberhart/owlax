    <!-- http://purl.org/ontology/olo/core#olo:item -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/olo/core#olo:item"/>
    


    <!-- http://purl.org/ontology/olo/core#olo:next -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/olo/core#olo:next"/>
    


    <!-- http://purl.org/ontology/olo/core#olo:previous -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/olo/core#olo:previous"/>
    


    <!-- http://purl.org/ontology/olo/core#olo:slot -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/olo/core#olo:slot"/>
    


    <!-- http://www.purl.org/drammar#achieves -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#achieves">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://www.purl.org/drammar#isAchievedBy"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>achieves links a Plan to the Goal it aims at achieving</rdfs:comment>
        <rdfs:label>achieves</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#appraisingAgent -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#appraisingAgent">
        <rdfs:domain rdf:resource="http://www.purl.org/drammar#Emotion"/>
        <rdfs:range rdf:resource="http://www.purl.org/drammar#Agent"/>
        <rdfs:comment>The appraisingAgent property connects an Emotion with the Agent who appraises
            it. It encompasses specific subproperties for the specific emotion types (in OCC theory,
            there are 22 emotion types such as Love or Shame)</rdfs:comment>
        <rdfs:label>appraisingAgent</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#coOccurInScene -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#coOccurInScene">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.purl.org/drammar#Plan"/>
        <rdfs:range rdf:resource="http://www.purl.org/drammar#Plan"/>
        <rdfs:comment>coOccurInScene can be used to describe the fact that two Plans co-occur in the
            same Scene.</rdfs:comment>
        <rdfs:label>coOccurInScene</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#containsFirstOLE -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#containsFirstOLE">
        <rdfs:subPropertyOf rdf:resource="http://www.purl.org/drammar#containsOLE"/>
        <rdfs:domain rdf:resource="http://www.purl.org/drammar#List"/>
        <rdfs:range rdf:resource="http://www.purl.org/drammar#OrderedListElement"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">First OLE of a
            List.</rdfs:comment>
        <rdfs:label>containsFirstOLE</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#containsLastOLE -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#containsLastOLE">
        <rdfs:subPropertyOf rdf:resource="http://www.purl.org/drammar#containsOLE"/>
        <rdfs:domain rdf:resource="http://www.purl.org/drammar#List"/>
        <rdfs:range rdf:resource="http://www.purl.org/drammar#OrderedListElement"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Last OLE of a
            List.</rdfs:comment>
        <rdfs:label>containsLastOLE</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#containsOLE -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#containsOLE">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/ontology/olo/core#olo:slot"/>
        <rdfs:subPropertyOf rdf:resource="http://www.purl.org/drammar#hasADTComponent"/>
        <owl:inverseOf rdf:resource="http://www.purl.org/drammar#isOLEContained"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.purl.org/drammar#List"/>
        <rdfs:range rdf:resource="http://www.purl.org/drammar#OrderedListElement"/>
        <rdfs:comment>Inverse of isOLElementOf. Connects the ordered list to its elements.
        </rdfs:comment>
        <rdfs:label>containsOLE</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#containsTN -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#containsTN">
        <rdfs:subPropertyOf rdf:resource="http://www.purl.org/drammar#hasADTComponent"/>
        <rdfs:domain rdf:resource="http://www.purl.org/drammar#Tree"/>
        <rdfs:range rdf:resource="http://www.purl.org/drammar#TreeNode"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Connects the tree to
            its (tree) nodes. E.g., the drama is a tree and the scenes are its tree
            nodes.</rdfs:comment>
        <rdfs:label>containsTN</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#describes -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#describes">
        <owl:inverseOf rdf:resource="http://www.purl.org/drammar#isDescribedBy"/>
        <rdfs:domain rdf:resource="http://www.purl.org/drammar#SituationSchema"/>
        <rdfs:range rdf:resource="http://www.purl.org/drammar#DramaPerdurant"/>
        <rdfs:comment>A SituationSchema describes a DramaPerdurant, i.e. provides a template for
            describing processes and states.</rdfs:comment>
        <rdfs:label>describes</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#feels -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#feels">
        <rdfs:subPropertyOf rdf:resource="http://www.purl.org/drammar#hasMentalState"/>
        <owl:inverseOf rdf:resource="http://www.purl.org/drammar#isEmotionOf"/>
        <rdfs:label>feels</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#follows -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#follows">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/ontology/olo/core#olo:next"/>
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://www.purl.org/drammar#precedes"/>
        <rdfs:comment>Inverse of precedes. Sequential order in an ordered list.</rdfs:comment>
        <rdfs:label>follows</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#happyFor_appraisingAgent -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#happyFor_appraisingAgent">
        <rdfs:subPropertyOf rdf:resource="http://www.purl.org/drammar#appraisingAgent"/>
        <rdfs:domain rdf:resource="http://www.purl.org/drammar#Emotion"/>
        <rdfs:range rdf:resource="http://www.purl.org/drammar#Agent"/>
        <rdfs:label>HappyFor_appraisingAgent</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#happyFor_target -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#happyFor_target">
        <rdfs:subPropertyOf rdf:resource="http://www.purl.org/drammar#target"/>
        <rdfs:domain rdf:resource="http://www.purl.org/drammar#Emotion"/>
        <rdfs:range rdf:resource="http://www.purl.org/drammar#Agent"/>
        <rdfs:label>happyFor_target</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#hasADTComponent -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#hasADTComponent">
        <rdfs:domain rdf:resource="http://www.purl.org/drammar#AbstractDataType"/>
        <rdfs:range rdf:resource="http://www.purl.org/drammar#ADTComponent"/>
        <rdfs:comment>A generic property for representing the relation among a given abstract
            structure type and its components.</rdfs:comment>
        <rdfs:label>hasADTComponent</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#hasChild -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#hasChild">
        <rdfs:subPropertyOf rdf:resource="http://www.purl.org/drammar#hasADTComponent"/>
        <owl:inverseOf rdf:resource="http://www.purl.org/drammar#hasParent"/>
        <rdfs:domain rdf:resource="http://www.purl.org/drammar#TreeNode"/>
        <rdfs:range rdf:resource="http://www.purl.org/drammar#TreeNode"/>
        <rdfs:comment>the basic relation employed for modelling the computational data structure of
            tree.</rdfs:comment>
        <rdfs:label>hasChild</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#hasData -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#hasData">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/ontology/olo/core#olo:item"/>
        <owl:inverseOf rdf:resource="http://www.purl.org/drammar#isDataOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.purl.org/drammar#ADTComponent"/>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment>Property that connects a structural element (e.g. of the class OLE) with the
            data contained in it. Range is not defined; it is defined for each specific structure
            element. </rdfs:comment>
        <rdfs:label>hasData</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#hasDoneState -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#hasDoneState">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://www.purl.org/drammar#isDoneStateOf"/>
        <rdfs:label>hasDoneState</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#hasEmotionType -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#hasEmotionType">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.purl.org/drammar#EmotionSchema"/>
        <rdfs:range rdf:resource="http://www.purl.org/drammar#ExternalRefEmotionType"/>
        <rdfs:label>hasEmotionType</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#hasExtRef -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#hasExtRef">
        <owl:inverseOf rdf:resource="http://www.purl.org/drammar#isExtRefOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.purl.org/drammar#DescriptionTemplate"/>
                    <rdf:Description rdf:about="http://www.purl.org/drammar#DramaEntity"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.purl.org/drammar#ExternalReference"/>
        <rdfs:comment>Quality of an entity (DramaEndurant) In particular: Object_type This property
            provides a URI, in YAGOSUMO or in Wordnet, or a custom string that defines the class of
            some object. E.g., object individual PinkOctopus in Oktapodi is of Object_type
            &quot;octopus&quot; in Yago. Environment_periodOfYear Epoch of some environment. E.g.
            summer, january, Easter. Can be a URI or forced custom string. This is why Range is
            empty. Environment_location Can be &quot;exterior&quot; or &quot;interior&quot;. From a
            fixed menu. Environment_timeOfDay Can be &quot;dawn&quot; &quot;morning&quot;
            &quot;afternoon&quot; &quot;evening&quot; &quot;night&quot;. From a fixed menu.
            Environment_type The type of an envrironment. E.g.: forest, kitchen, office, ... . It
            can be custom string or URI. This is why Range is empty. Environment_epoch Epoch of some
            environment. E.g. 20th century, 1710&apos;s, 50&apos;s. Can be a URI or forced custom
            string. This is why Range is empty. </rdfs:comment>
        <rdfs:label>hasExtRef</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#hasFiller -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#hasFiller">
        <owl:inverseOf rdf:resource="http://www.purl.org/drammar#isFillerOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.purl.org/drammar#Role"/>
        <rdfs:range rdf:resource="http://www.purl.org/drammar#DramaEntity"/>
        <rdfs:comment>Filler is the property that connects a Role of a FrameNetSchema with the
            DramaEntity (DramaEndurant or DramaPerdurant) that fills that Role.</rdfs:comment>
        <rdfs:label>hasFiller</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#hasGoal -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#hasGoal">
        <rdfs:subPropertyOf rdf:resource="http://www.purl.org/drammar#hasMentalState"/>
        <owl:inverseOf rdf:resource="http://www.purl.org/drammar#isGoalOf"/>
        <rdfs:domain rdf:resource="http://www.purl.org/drammar#Agent"/>
        <rdfs:range rdf:resource="http://www.purl.org/drammar#Goal"/>
        <rdfs:comment>The property which connects an agent with her/his goals.</rdfs:comment>
        <rdfs:label>hasGoal</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#hasMember -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#hasMember">
        <rdfs:subPropertyOf rdf:resource="http://www.purl.org/drammar#hasADTComponent"/>
        <owl:inverseOf rdf:resource="http://www.purl.org/drammar#isMemberOf"/>
        <rdfs:domain rdf:resource="http://www.purl.org/drammar#Set"/>
        <rdfs:range rdf:resource="http://www.purl.org/drammar#SetMember"/>
        <rdfs:comment>The specific property for modelling Sets (as a data structure). Any entity, in
            Drammar, can be a member of a Set.</rdfs:comment>
        <rdfs:label>hasMember</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#hasMentalState -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#hasMentalState">
        <rdfs:domain rdf:resource="http://www.purl.org/drammar#Agent"/>
        <rdfs:range rdf:resource="http://www.purl.org/drammar#MentalState"/>
        <rdfs:label>hasMentalState</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#hasMotivationIn -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#hasMotivationIn">
        <owl:inverseOf rdf:resource="http://www.purl.org/drammar#isMotivationFor"/>
        <rdfs:comment>The hasMotivation property represents the relation bewteen a plan and the
            timeline it motivates (namely, where the actions of the plan are actually
            contained).</rdfs:comment>
        <rdfs:label>hasMotivationIn</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#hasParent -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#hasParent">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>The parent/child relation for modelling tree structures.</rdfs:comment>
        <rdfs:label>hasParent</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#hasPlanEffect -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#hasPlanEffect">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://www.purl.org/drammar#isPlanEffectOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.purl.org/drammar#Plan"/>
        <rdfs:range rdf:resource="http://www.purl.org/drammar#ConsistentStateSet"/>
        <rdfs:comment>Property that connects a plan with the consistent set of states that is the
            effect of a plan (ConsistentStateSet).</rdfs:comment>
        <rdfs:label>hasPlanEffect</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#hasPlanPrecondition -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#hasPlanPrecondition">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://www.purl.org/drammar#isPlanPreconditionOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.purl.org/drammar#Plan"/>
        <rdfs:range rdf:resource="http://www.purl.org/drammar#ConsistentStateSet"/>
        <rdfs:comment>Property that connect a plan with the ConsistentStateState which forms its
            precondition.</rdfs:comment>
        <rdfs:label>hasPlanPrecondition</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#hasPropositionalContent -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#hasPropositionalContent">
        <owl:inverseOf rdf:resource="http://www.purl.org/drammar#isPropositionalContentOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.purl.org/drammar#BeliefSchema"/>
                    <rdf:Description rdf:about="http://www.purl.org/drammar#GoalSchema"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:comment>hasPropositionalContent links a BeliefSchema or a GoalSchema to the its
            content (a factual state or process), that is, the Process or State that is the actual
            content of the belief or the goal.</rdfs:comment>
        <rdfs:label>hasPropositionalContent</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#hasRole -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#hasRole">
        <owl:inverseOf rdf:resource="http://www.purl.org/drammar#isRoleOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.purl.org/drammar#FrameNetSchema"/>
        <rdfs:range rdf:resource="http://www.purl.org/drammar#Role"/>
        <rdfs:comment>Connects a SituationSchema ( modelled by a frame-like linguistic structure)
            with each Role of the schema.</rdfs:comment>
        <rdfs:label>hasRole</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#hasRoot -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#hasRoot">
        <rdfs:subPropertyOf rdf:resource="http://www.purl.org/drammar#containsTN"/>
        <rdfs:domain rdf:resource="http://www.purl.org/drammar#Tree"/>
        <rdfs:range rdf:resource="http://www.purl.org/drammar#TreeNode"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Connects the Tree with
            the TreeNode that is its Root.</rdfs:comment>
        <rdfs:label>hasRoot</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#hasTimelineEffect -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#hasTimelineEffect">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://www.purl.org/drammar#isTimelineEffectOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.purl.org/drammar#Timeline"/>
        <rdfs:range rdf:resource="http://www.purl.org/drammar#ConsistentStateSet"/>
        <rdfs:comment>Property that connect a Timeline with the ConsistentStateState which forms its
            effects.</rdfs:comment>
        <rdfs:label>hasTimelineEffect</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#hasTimelinePrecondition -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#hasTimelinePrecondition">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://www.purl.org/drammar#isTimelinePreconditionOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.purl.org/drammar#Timeline"/>
        <rdfs:range rdf:resource="http://www.purl.org/drammar#ConsistentStateSet"/>
        <rdfs:comment>Property that connect a Timeline with the ConsistentStateState which forms its
            precondition.</rdfs:comment>
        <rdfs:label>hasTimelinePrecondition</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#hasValueEngaged -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#hasValueEngaged">
        <rdfs:subPropertyOf rdf:resource="http://www.purl.org/drammar#hasMentalState"/>
        <owl:inverseOf rdf:resource="http://www.purl.org/drammar#isValueEngagedOf"/>
        <rdfs:domain rdf:resource="http://www.purl.org/drammar#Agent"/>
        <rdfs:range rdf:resource="http://www.purl.org/drammar#Value"/>
        <rdfs:comment>The property hasValue connects an agent to her/his values</rdfs:comment>
        <rdfs:label>hasValueEngaged</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#hingesOn -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#hingesOn">
        <owl:inverseOf rdf:resource="http://www.purl.org/drammar#isHingedOnBy"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.purl.org/drammar#DrammarScene"/>
        <rdfs:range rdf:resource="http://www.purl.org/drammar#ConflictSet"/>
        <rdfs:comment>A DrammarScene hinges on some ConflictSet</rdfs:comment>
        <rdfs:label>hingesOn</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#inConflictWith -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#inConflictWith">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.purl.org/drammar#Plan"/>
        <rdfs:range rdf:resource="http://www.purl.org/drammar#Plan"/>
        <rdfs:comment>The conflict relation over plans. The ontology is neutral with respect with
            conflicts over other entity types, such a goals: it only assumes that conflicts over
            plans are observable.</rdfs:comment>
        <rdfs:label>inConflictWith</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#inSupportOf -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#inSupportOf">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.purl.org/drammar#Plan"/>
        <rdfs:range rdf:resource="http://www.purl.org/drammar#Plan"/>
        <rdfs:comment>The support relation over goals (parallel to inConflictWith)</rdfs:comment>
        <rdfs:label>inSupportOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#intends -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#intends">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://www.purl.org/drammar#isIntendedBy"/>
        <rdfs:domain rdf:resource="http://www.purl.org/drammar#Agent"/>
        <rdfs:range rdf:resource="http://www.purl.org/drammar#Plan"/>
        <rdfs:comment>Intends has as domain an Agent and as range a Plan.</rdfs:comment>
        <rdfs:label>intends</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#isAchievedBy -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#isAchievedBy">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.purl.org/drammar#Goal"/>
        <rdfs:range rdf:resource="http://www.purl.org/drammar#Plan"/>
        <rdfs:comment>This property connects a goal with the plan that achieves it.</rdfs:comment>
        <rdfs:label>isAchievedBy</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#isDataOf -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#isDataOf">
        <rdfs:comment>isDataOf connects some entity to a data structure component (namely, an
            OrderedListElement) of which it is the content.</rdfs:comment>
        <rdfs:label>isDataOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#isDescribedBy -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#isDescribedBy">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Inverse of describes, this property connects a Dynamics (Process or State)
            with the SituationSchema that describes it.</rdfs:comment>
        <rdfs:label>isDescribedBy</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#isDoneStateOf -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#isDoneStateOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.purl.org/drammar#Plan"/>
                    <rdf:Description rdf:about="http://www.purl.org/drammar#Process"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment>This is the property that connects a process with a particular state, called a
            done state, that holds whether the process has terminated. </rdfs:comment>
        <rdfs:label>isDoneStateOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#isEmotionOf -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#isEmotionOf">
        <rdfs:subPropertyOf rdf:resource="http://www.purl.org/drammar#isMentalStateOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.purl.org/drammar#Emotion"/>
        <rdfs:range rdf:resource="http://www.purl.org/drammar#Agent"/>
        <rdfs:comment>Connects an emotion with the agent who feels it.</rdfs:comment>
        <rdfs:label>isEmotionOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#isExtRefOf -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#isExtRefOf">
        <rdfs:label>isExtRefOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#isExtRefRoleOfExtRefSchema -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#isExtRefRoleOfExtRefSchema">
        <rdfs:domain rdf:resource="http://www.purl.org/drammar#ExternalRefRole"/>
        <rdfs:range rdf:resource="http://www.purl.org/drammar#ExternalRefSchema"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.purl.org/drammar#isExtRefOf"/>
            <rdf:Description rdf:about="http://www.purl.org/drammar#isRoleOf"/>
            <rdf:Description rdf:about="http://www.purl.org/drammar#hasExtRef"/>
        </owl:propertyChainAxiom>
        <rdfs:comment>This property chain serves the purpose of connecting a frame role with the
            frame it belongs to, via the following property chain: isExtRefOf o isRoleOf o hasExtRef
            where - isExtRefOf connects ExtRefRole to Role - isRoleOf connects Role to ProcessSchema
            - hasExtRef connects ProcessSchema to ExtRefSchema (remember that the actual reference
            to the frame or frame role in an external resources are dealt with by attaching a
            &quot;quale&quot; data property to the classes that represent the external
            references)</rdfs:comment>
        <rdfs:label>isExtRefRoleOfExtRefSchema</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#isFillerOf -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#isFillerOf">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:comment>Connects some DramaEndurant or Perdurant to a Role in a
            SituationSchema</rdfs:comment>
        <rdfs:label>isFillerOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#isGoalOf -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#isGoalOf">
        <rdfs:subPropertyOf rdf:resource="http://www.purl.org/drammar#isMentalStateOf"/>
        <rdfs:range rdf:resource="http://www.purl.org/drammar#Agent"/>
        <rdfs:comment>inverse of hasGoal</rdfs:comment>
        <rdfs:label>isGoalOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#isHingedOnBy -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#isHingedOnBy">
        <rdfs:comment>Inverse of hingesOn.</rdfs:comment>
        <rdfs:label>isHingedOnBy</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#isIntendedBy -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#isIntendedBy">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Inverse of intends.</rdfs:comment>
        <rdfs:label>isIntendedBy</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#isKnownBy -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#isKnownBy">
        <rdfs:subPropertyOf rdf:resource="http://www.purl.org/drammar#isMentalStateOf"/>
        <owl:inverseOf rdf:resource="http://www.purl.org/drammar#knows"/>
        <rdfs:range rdf:resource="http://www.purl.org/drammar#Agent"/>
        <rdfs:label>isKnownBy</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#isMemberOf -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#isMemberOf">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:label>isMemberOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#isMentalStateOf -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#isMentalStateOf">
        <rdfs:label>isMentalStateOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#isMotivationFor -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#isMotivationFor">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.purl.org/drammar#Plan"/>
        <rdfs:range rdf:resource="http://www.purl.org/drammar#Timeline"/>
        <rdfs:comment>A plan is the motivation for a timeline, namely for a list of units containing
            actions. This property is functional: so, a plan can motivate only a timeline. Notice
            that this implies that a plan cannot motivate the same timeline motivated by a sub-plan,
            and that a plan cannot motivate the same timeline motivated by a more abstract plan. Of
            course, the fact that this property is functional does not imply that a single timeline
            cannot be motivated by more plans.</rdfs:comment>
        <rdfs:label>isMotivationFor</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#isOLEContained -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#isOLEContained">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Connects the element of an ordered list to the ordered list.</rdfs:comment>
        <rdfs:label>isOleContained</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#isOrderedBy -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#isOrderedBy">
        <owl:inverseOf rdf:resource="http://www.purl.org/drammar#orders"/>
        <rdfs:label>isOrderedBy</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#isPlanEffectOf -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#isPlanEffectOf">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:label>isPlanEffectOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#isPlanPreconditionOf -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#isPlanPreconditionOf">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:label>isPlanPreconditionOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#isPropositionalContentOf -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#isPropositionalContentOf">
        <rdfs:comment>Inverse of hasPropositionalContent. It links a BeliefSchema or a GoalSchema to
            a State or Process.</rdfs:comment>
        <rdfs:label>isPropositionalContentOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#isRoleOf -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#isRoleOf">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:label>isRoleOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#isSpannedBy -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#isSpannedBy">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://www.purl.org/drammar#spans"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:label>isSpannedBy</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#isTimelineEffectOf -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#isTimelineEffectOf">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:label>isTimelineEffectOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#isTimelinePreconditionOf -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#isTimelinePreconditionOf">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:label>isTimelinePreconditionOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#isValueEngagedOf -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#isValueEngagedOf">
        <rdfs:subPropertyOf rdf:resource="http://www.purl.org/drammar#isMentalStateOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.purl.org/drammar#Agent"/>
        <rdfs:label>isValueEngagedOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#knows -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#knows">
        <rdfs:subPropertyOf rdf:resource="http://www.purl.org/drammar#hasMentalState"/>
        <rdfs:domain rdf:resource="http://www.purl.org/drammar#Agent"/>
        <rdfs:range rdf:resource="http://www.purl.org/drammar#Belief"/>
        <rdfs:comment>Property of an agent, who BELIEVES a Belief (a mental state with a
            propositional content). BELIEVES = KNOWS.</rdfs:comment>
        <rdfs:label>knows</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#love_appraisingAgent -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#love_appraisingAgent">
        <rdfs:subPropertyOf rdf:resource="http://www.purl.org/drammar#appraisingAgent"/>
        <rdfs:domain rdf:resource="http://www.purl.org/drammar#Emotion"/>
        <rdfs:range rdf:resource="http://www.purl.org/drammar#Agent"/>
        <rdfs:label>love_appraisingAgent</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#love_target -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#love_target">
        <rdfs:subPropertyOf rdf:resource="http://www.purl.org/drammar#target"/>
        <rdfs:domain rdf:resource="http://www.purl.org/drammar#Emotion"/>
        <rdfs:range rdf:resource="http://www.purl.org/drammar#Agent"/>
        <rdfs:label>love_target</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#orders -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#orders">
        <rdfs:domain rdf:resource="http://www.purl.org/drammar#Timeline"/>
        <rdfs:range rdf:resource="http://www.purl.org/drammar#Unit"/>
        <rdfs:comment>A Timeline orders Unit.</rdfs:comment>
        <rdfs:label>orders</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#precedes -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#precedes">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/ontology/olo/core#olo:previous"/>
        <rdfs:domain rdf:resource="http://www.purl.org/drammar#OrderedListElement"/>
        <rdfs:range rdf:resource="http://www.purl.org/drammar#OrderedListElement"/>
        <rdfs:comment>Sequential order in an ordered list.</rdfs:comment>
        <rdfs:label>precedes</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#shame_appraisingAgent -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#shame_appraisingAgent">
        <rdfs:subPropertyOf rdf:resource="http://www.purl.org/drammar#appraisingAgent"/>
        <rdfs:domain rdf:resource="http://www.purl.org/drammar#Emotion"/>
        <rdfs:range rdf:resource="http://www.purl.org/drammar#Agent"/>
        <rdfs:label>shame_appraisingAgent</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#shame_target -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#shame_target">
        <rdfs:subPropertyOf rdf:resource="http://www.purl.org/drammar#target"/>
        <rdfs:domain rdf:resource="http://www.purl.org/drammar#Emotion"/>
        <rdfs:range rdf:resource="http://www.purl.org/drammar#Agent"/>
        <rdfs:label>shame_target</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#spans -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#spans">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.purl.org/drammar#Scene"/>
        <rdfs:range rdf:resource="http://www.purl.org/drammar#Timeline"/>
        <rdfs:comment>The coincidence relation of a Scene on a Timeline.</rdfs:comment>
        <rdfs:label>spans</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/drammar#target -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/drammar#target">
        <rdfs:comment>A generic property for describing the relation of an appraised emotional state
            with the target of the emotion, further articulated into more specific target types for
            each emotion type (see appraisingAgent property).</rdfs:comment>
        <rdfs:label>target</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/2002/07/owl#topObjectProperty -->

    <owl:ObjectProperty rdf:about="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    


